					<setvar0 a="1" b="1" op="div" />
					<clientuitrigger name="bossHealthbar_Update" value="var0"/>
				</condition>	
					<else>
						<condition test="hasstate Bastact1_State_Executioner2_Stage2" target="target_entity" >
							<setvar0 a=".65" b="target_maxhealth" op="mult" />
