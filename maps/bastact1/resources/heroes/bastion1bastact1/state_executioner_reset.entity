							<expirestate name="State_Bastion1Bastact1_ExecutionerReset" target="target_entity"/>
							
							<!-- Boss Healthbar Reset -->
							<setvar0 a=".65" b="1" op="div" />
							<clientuitrigger name="bossHealthbar_Update" value="var0"/>
						</condition>	
						<else>	
							<condition test="hasstate Bastact1_State_Executioner2_Stage3" target="target_entity" >
								<setvar0 a=".25" b="target_maxhealth" op="mult" />
								<changehealth source="target_entity" target="target_entity" a="var0" b="target_health" op="sub" />	
								<expirestate name="State_Bastion1Bastact1_ExecutionerReset" target="target_entity"/>
								
								<!-- Boss Healthbar Reset -->
								<setvar0 a=".25" b="1" op="div" />
								<clientuitrigger name="bossHealthbar_Update" value="var0"/>
							</condition>
						</else>
					</else>	
		</oninflict>	


	
</state>