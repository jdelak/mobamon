<?xml version="1.0" encoding="UTF-8"?>
<state
	name="State_Bastion1Bastact1_ExecutionerReset"

	icon="icon.tga"
	passiveeffect=""

	effecttype=""
	ishidden="true"
>	

		<oninflict>
				<condition test="hasstate Bastact1_State_Executioner2_Stage1" target="target_entity" >
					<setvar0 a="1" b="target_maxhealth" op="mult" />
