<?xml version="1.0" encoding="UTF-8"?>
<model name="" file="model.model" type="K2">
	<anim name="idle" clip="default_1.clip" fps="30" loop="true">
		<startevent frame="1" cmd="StartEffect effects/fireplace_flame.effect"/>
	</anim>
	
	<anim name="crack" clip="crack.clip" fps="30" loop="true" loopbackframe="70">
		<startevent frame="1" cmd="StartEffect effects/hearthbreak.effect"/>
	</anim>	
	
	<anim name="present" clip="present.clip" fps="30" loop="true" loopbackframe="120"/>
</model>
