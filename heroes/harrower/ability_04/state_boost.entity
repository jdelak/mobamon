<?xml version="1.0" encoding="UTF-8"?>
<state
	name="State_Harrower_Ability4_Speed"
	
	icon="icon.tga"
	
	movespeedpercharge="20"
	maxcharges="10"
	
	effecttype="StatusBuff"
	allowtransfer="true"
	impactinterval="350"
	ishidden="true"
>
	<oninflict>
		<setcharges a="10" />
	</oninflict>

	<onimpact>
		<setaccumulator value="1" />
	</onimpact>

	<onframe>
		<compare a="accumulator" b="1" op="eq" >
			<removecharge a="1" />
		</compare>
	</onframe>
</state>
