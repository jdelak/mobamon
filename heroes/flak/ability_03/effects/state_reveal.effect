					startsize="750"
					endsize="0"
					material="/shared/effects/smaterials/whitedot.material"
					lockup="1"
					lockright="1"
					pitch="90"
					depthbias="-50"
					rollspeed="{zrollspeed}"
					roll="{zroll}"
					position="0 0 400"
				/>

			</template>
			
			<instance name="" zroll="45" zrollspeed="0"/>
			
			
			
			

			

		</particlesystem>
			

			
			
			
			
			
			
			
			
			
			
			
		<particlesystem name="system2" space="entity" scale=".5">

			<simpleemitter
				position="0 0 400"
				spawnrate="4"
				minparticlelife="2000"
				maxparticlelife="2000"
				gravity="0"
				acceleration="0"
				timenudge="2000"
				material="/shared/effects/smaterials/whitedot_ground_inverted.material"
				offsetsphere="0 0 0"
			>
				<particle
					anchor="1"
					startcolor="0 0 0"
					midcolor="0 .125 .15"
					endcolor="0 0 0"
					midcolorpos=".25"
					startscale="0"
					minendscale=".5"
					maxendscale=".5"
					width="1600"
					height="1600"
					lockup="1"
					lockright="1"
					pitch="90"
				/>
			</simpleemitter>

			<simpleemitter
				position="0 0 400"
				spawnrate="1"
				particlelife="2000"
				direction="0 0 1"
				timenudge="2000"
				minangle="0"
				maxangle="1"
				speed="0"
			>
				<particle
				>
				
					<template name="">
						<billboard
							position="0 0 0"
							material="/shared/effects/smaterials/bulb_add.material"
							startcolor="0 0 0"
							midcolor="0 .05 .075"
							endcolor="0 0 0"
							life="600"
							startsize="550"
							endsize="600"
							lockup="1"
							lockright="1"
							pitch="90"
							roll="{zroll}"
							depthbias="-65"
						/>
						
						<billboard
							position="0 0 0"
							material="/shared/effects/smaterials/bulb_add.material"
							startcolor="0 0 0"
							midcolor="0 .05 .075"
							endcolor="0 0 0"
							life="600"
							startsize="400"
							endsize="450"
							lockup="1"
							lockright="1"
							pitch="90"
							roll="{zroll}"
							depthbias="-65"
						/>
						
						<billboard
							position="0 0 0"
							material="/shared/effects/smaterials/bulb_add.material"
							startcolor="0 0 0"
							midcolor="0 .15 .15"
							endcolor="0 0 0"
							life="300"
							startsize="250"
							endsize="600"
							lockup="1"
							lockright="1"
							pitch="90"
							roll="{zroll}"
							depthbias="-65"
						/>
					</template>
					
					<instance name="" zroll="0"/>
					<instance name="" zroll="180"/>
					
				</particle>
			</simpleemitter>

		</particlesystem>
		
	</definitions>
	<thread>
		<spawnparticlesystem instance="instance0" particlesystem="system0" />
		<spawnparticlesystem instance="instance1" particlesystem="system1" />
		<spawnparticlesystem instance="instance2" particlesystem="system2" />
		<waitfordeath instance="instance0" />
		<waitfordeath instance="instance1" />
		<waitfordeath instance="instance2" />
	</thread>
</effect>
