			<instance zmaterial="/shared/effects/smaterials/radar_quad2.material" zdelay="3650" zlife="3000" zcolor="0 .125 .25"/>
			



		

			<template name="">
			
				<billboard
					expirelife="1250"
					startcolor=".15 .3 .3"
					endcolor="0 0 0"
					size="540"
					material="/shared/effects/smaterials/radar.material"
					lockup="1"
					lockright="1"
					pitch="-90"
					depthbias="-50"
					rollspeed="-90"
					position="0 0 400"
				/>
				
				<billboard
					expirelife="1250"
					startcolor="0 .075 .1"
					endcolor="0 0 0"
					size="540"
					material="/shared/effects/smaterials/radar_target.material"
					lockup="1"
					lockright="1"
					pitch="-90"
					depthbias="-50"

					position="0 0 400"
				/>
