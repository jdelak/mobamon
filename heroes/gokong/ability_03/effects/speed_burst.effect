<?xml version="1.0" encoding="UTF-8"?>
<effect deferred="true" useentityeffectscale="1">
	<definitions>
	
		<particlesystem name="system0" space="world" scale=".6">

			<sound
				falloffstart="500"
				falloffend="2500"
				volume="0.4"
				sample="../sounds/speed.wav"
			/>
			
			
			
			
			<billboard
				life="250"
				position="0 0 0"
				material="/shared/effects/smaterials/whitedot.material"
				startcolor="1 1 1"
				endcolor="0 0 0"
				scale="3"
				directionalspace="local"
				lockright="1"
				lockup="1"
				pitch="90"
				depthbias="-25"
			/>	
			
			
			
			
			
			<template name="smoke">
