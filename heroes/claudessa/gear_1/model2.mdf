<?xml version="1.0" encoding="UTF-8"?>
<model name="rig.max" file="high.model" type="K2" high="high.model" med="med.model" low="low.model">

	<skin name="black">
		<reference mesh="10k_claudessa"			material="material_black.material" />
		<reference mesh="3k_claudessa"				material="material_black.material" />
		<reference mesh="1k_claudessa"				material="material_black.material" />
	</skin>		
	
	<skin name="flames">
		<reference mesh="10k_claudessa"			material="material_flames.material" />
		<reference mesh="3k_claudessa"				material="material_flames.material" />
		<reference mesh="1k_claudessa"				material="material_flames.material" />
	</skin>	
	
	<skin name="pink">
		<reference mesh="10k_claudessa"			material="material_pink.material" />
		<reference mesh="3k_claudessa"				material="material_pink.material" />
		<reference mesh="1k_claudessa"				material="material_pink.material" />
	</skin>		
	

	
	<anim name="walk_1" clip="clips/walk_1_shield.clip" loop="true">
		<frameevent frame="3" cmd="PlaySoundLinear sounds/sfx_foley_%.wav 500 1200 0.2"/>
		<frameevent frame="6" cmd="StartEffect /shared/effects/footstep_l_#GetTerrainType()#.effect"/>
		<frameevent frame="17" cmd="PlaySoundLinear sounds/sfx_foley_%.wav 500 1200 0.2"/>
		<frameevent frame="20" cmd="StartEffect /shared/effects/footstep_l_#GetTerrainType()#.effect"/>
		<frameevent frame="34" cmd="PlaySoundLinear sounds/sfx_foley_%.wav 500 1200 0.2"/>
		<frameevent frame="37" cmd="StartEffect /shared/effects/footstep_l_#GetTerrainType()#.effect"/>
		<frameevent frame="47" cmd="PlaySoundLinear sounds/sfx_foley_%.wav 500 1200 0.2"/>
		<frameevent frame="50" cmd="StartEffect /shared/effects/footstep_l_#GetTerrainType()#.effect"/>
	</anim>
	
	
	
	
	<anim name="ability_1" clip="clips/ability_1_shield.clip" />

	<anim name="ability_2" clip="clips/ability_2_shield.clip" />
	
	<anim name="ability_3" clip="clips/ability_3_shield.clip" >
		<frameevent frame="1" cmd="StartEffect ability_03/effects/hero_swoosh_shield.effect 0"/>
		<frameevent frame="10" cmd="PlaySoundLinear sounds/voice/vo_grunt_%.wav 500 2500 0.55" />
	</anim>
	

	<anim name="ability_4" clip="clips/ability_4_shield.clip" />
	
	<anim name="attack_1" clip="clips/attack_1_shield.clip" />

	<anim name="attack_2" clip="clips/attack_2_shield.clip" />
	
	<anim name="bored_1" clip="clips/bored_1_shield.clip" />
	
	<anim name="death_1" clip="clips/death_1.clip" loop="true" loopbackframe="-1">
		<frameevent frame="1" cmd="PlaySoundLinear sounds/voice/vo_death_grunt_%.wav 500 2500 0.75" />
		<frameevent frame="1" cmd="PlaySoundLinear sounds/sfx_death_foley.wav 500 2500 0.55" />
	</anim>
	
	<anim name="idle" clip="clips/default_1_shield.clip" loop="true"/>
	
	<anim name="emote_1" clip="clips/emote_1_shield.clip" fps="30" loop="false">
		<frameevent frame="1" cmd="PlaySoundLinear sounds/voice/vo_emote_%.wav 500 2500 0.75"/>
	</anim>

	<anim name="item_1" clip="clips/item_1_shield.clip"/>
	
	<anim name="item_channel_1" clip="clips/item_channel_1_shield.clip" loop="true"/>

	<anim name="item_self_1" clip="clips/item_self_1_shield.clip"/>

	<anim name="port_in" clip="clips/port_in_shield.clip" blendtime="0"/>
	
	<anim name="port_out" clip="clips/port_out_shield.clip" />
	
	<anim name="select_1" clip="clips/select_1_shield.clip" loop="true"/>
	
	<anim name="spawn_1" clip="clips/spawn_1_shield.clip"/>
	
	<anim name="slide_1" clip="clips/slide_1_shield.clip"/>
	
	<anim name="stunned_1" clip="clips/stunned_1_shield.clip" loop="true"/>



</model>