<?xml version="1.0" encoding="UTF-8"?>
<node name="view_normal">
	<dependency name="ObjectNormal" type="float3" source="ObjectNormal" />
	<dependency name="mWorldViewRotate" type="float3x3" source="mWorldViewRotate" />
	<output name="Out" type="float3" solution="ViewNormal" />
	<?hlsl
		Out = mul(ObjectNormal, mWorldViewRotate);
	?>
	<?glsl
		Out = mWorldViewRotate * ObjectNormal;
	?>
</node>
