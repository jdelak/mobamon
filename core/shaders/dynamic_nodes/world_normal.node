<?xml version="1.0" encoding="UTF-8"?>
<node name="world_normal">
	<dependency name="ObjectNormal" type="float3" source="ObjectNormal" />
	<dependency name="mWorldRotate" type="float3x3" source="mWorldRotate" />
	<output name="Out" type="float3" solution="WorldNormal" />
	<?hlsl
		Out = mul(ObjectNormal, mWorldRotate);
	?>
	<?glsl
		Out = mWorldRotate * ObjectNormal;
	?>
</node>
