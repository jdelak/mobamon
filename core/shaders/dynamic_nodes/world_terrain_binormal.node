<?xml version="1.0" encoding="UTF-8"?>
<node name="world_terrain_binormal">
	<dependency name="Normal" type="float3" source="WorldNormal" />
	<dependency name="Tangent" type="float3" source="WorldTerrainTangent" />
	<dependency name="TangentSign" type="float" source="ObjectTerrainTangentSign" />
	<output name="Out" type="float3" solution="WorldTerrainBinormal" />
	<?hlsl
		Out = cross(Tangent, Normal) * TangentSign;
	?>
	<?glsl
		Out = cross(Tangent, Normal) * TangentSign;
	?>
</node>
