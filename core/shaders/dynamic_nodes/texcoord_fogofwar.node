<?xml version="1.0" encoding="UTF-8"?>
<node name="texcoord_fogofwar">
	<dependency name="ObjectPosition" type="float3" source="ObjectPosition" />
	<dependency name="mFowProj" type="float4x4" source="mFowProj" />
	<output name="Out" type="float2" solution="TexcoordFogofwar" />
	<?hlsl
		Out = mul(float4(ObjectPosition, 1.0f), mFowProj).xy;
	?>
	<?glsl
		Out = (mFowProj * vec4(ObjectPosition, 1.0f)).xy;
	?>
</node>
