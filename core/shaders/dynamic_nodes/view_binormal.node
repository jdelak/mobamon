<?xml version="1.0" encoding="UTF-8"?>
<node name="view_binormal">
	<dependency name="Normal" type="float3" source="ViewNormal" />
	<dependency name="Tangent" type="float3" source="ViewTangent" />
	<dependency name="TangentSign" type="float" source="ObjectTangentSign" />
	<output name="Out" type="float3" solution="ViewBinormal" />
	<?hlsl
		Out = cross(Tangent, Normal) * TangentSign;
	?>
	<?glsl
		Out = cross(Tangent, Normal) * TangentSign;
	?>
</node>
