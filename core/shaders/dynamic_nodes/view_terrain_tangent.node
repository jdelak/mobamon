<?xml version="1.0" encoding="UTF-8"?>
<node name="view_terrain_tangent">
	<dependency name="ObjectTangent" type="float3" source="ObjectTerrainTangent" />
	<dependency name="mWorldViewRotate" type="float3x3" source="mWorldViewRotate" />
	<output name="Out" type="float3" solution="ViewTerrainTangent" />
	<?hlsl
		Out = mul(ObjectTangent, mWorldViewRotate);
	?>
	<?glsl
		Out = mWorldViewRotate * ObjectTangent;
	?>
</node>
