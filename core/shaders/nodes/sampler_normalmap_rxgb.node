<?xml version="1.0" encoding="UTF-8"?>
<node name="sampler_normalmap_rxgb" novs="true">
	<parameter name="Sampler" type="sampler2DMulti" />
	<parameter name="Index" type="TexcoordIndex" default="0" />
	<dependency name="Texcoord" type="float2" source="Index" />
	<dependency name="Sampler_0" type="sampler2D" source="Sampler" sourceindex="0" />
	<dependency name="Sampler_1" type="sampler2D" source="Sampler" sourceindex="1" />
	<dependency name="ShaderModel" type="*" source="ShaderModel" />
	<output name="Out" type="float3" />
	<output name="a" type="float" />
	<case>
