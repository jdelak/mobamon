		?>
	</case>
	<case>
		<dependency name="ShaderModel" type="glsl120" source="ShaderModel" />
		<?glsl
			vec3 f3NormalmapColor = texture2D(Sampler_0, Texcoord).agb;
			Out = vec3(f3NormalmapColor * 2.0f - 1.0f);
