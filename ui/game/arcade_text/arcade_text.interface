			/>
<!--
			<image texture="/ui/game/arcade_text/textures/generator.tga" y="-150%" align="center" height="300%" width="300@" noclick="1" />
-->
			<!-- Title -->
			<label name="center_announcements_generator_title" wrap="1" textalign="center" y='-20%' x="-50%" textvalign="center" align="center" style="h1" font="maindyn_26" color="0.7 0 0" noclick="1" outline="1" outlinecolor="0.10 0 0 0.9" content="Generator" visible="0"/>
			<label name="center_announcements_generator_defeat" wrap="1" textalign="center" y='10%' x="50%" textvalign="center" align="center" style="h1" font="maindyn_36" color="1 0 0 1" noclick="1" outline="1" outlinecolor="0.1 0.1 0.1 0.9" content="Destroyed!" visible="0"/>

		</panel>

		<!-- Your hero respawned (off screen) -->
		<panel name="center_announcements_respawn" y="18h" align="center" height="28.0h" width="28h"  noclick="1" visible="0">

			<!-- Title -->
			<label name="center_announcements_respawn_title" wrap="1" y="-55%" textalign="center" textvalign="center" align="center" style="h1" font="maindyn_26" color="0 0.75 1 1" noclick="1" content="You Have" visible="0"/>
			<label name="center_announcements_respawn_sub" y="-40%" textalign="center" textvalign="center" align="center" style="h1" font="maindyn_48" color="1 1 1 1" noclick="1" content="Respawned" visible="0"/>
		</panel>

		<!-- Victory -->
		<panel name="center_announcements_victory" y="25h" align="center" height="11.0h" width="44.0h" noclick="1" visible="0">
			<!-- Glow - To be replaced by effect -->
			<image name="center_announcements_victory_glow" texture="/ui/game/arcade_text/textures/baldir_defeat_glow.tga" y="-150%" align="center" height="300%" width="300@" color="0 1 0 .8" rendermode="additive" noclick="1" />

			<modelpanel name="center_announcements_victory_model" y="-35%" width="+110h" height="+150h" align="center" valign="center" noclick="1" visible="1"
