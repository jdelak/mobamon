				cameraangles="0 0 180" camerafov="90"

			/>

			<image texture="/ui/game/arcade_text/textures/baldir_defeat.tga" y="-150%" align="center" height="300%" width="300@" noclick="1" />

			<!-- Title -->
			<label name="center_announcements_victory_title" wrap="1" y="10%" textalign="center" textvalign="center" align="center" style="h1" font="maindyn_72" color="0 1 0 1" outline="1" outlinecolor="0.04 0.24 0.03 0.9" noclick="1" content="events_victory" visible="0"/>

		</panel>

		<!-- Defeat -->
		<panel name="center_announcements_defeat" y="25h" align="center" height="11.0h" width="44.0h" noclick="1" visible="0">
