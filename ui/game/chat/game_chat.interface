	<!-- Overlays (round/border icon)  -->
	<panel group="gameChatContainers" style="gameChatContainer">
		<panel group="gameChatViewArea" style="gameChatViewArea">
			<panel group="gameChatBody" style="gameChatBody">
				<instance name="gameChatEntryOverlays" id="30"/>
				<instance name="gameChatEntryOverlays" id="29"/>
				<instance name="gameChatEntryOverlays" id="28"/>
