			</panel>
		</panel>

		<panel group="gameChatInputContainers" style="gameChatInputSpace">
			<!--<label name="gameChatTypeLabel" style="gameChatTypeSize" x="0.25h" height="2.5h" font="maindyn_12" shadow="true" color="white" textalign="center" noclick="true" content="game_chat_team"/>-->
			<panel name="gameChatInputChannelContainer">
				<frame texture="/ui/elements:rounded_bg_white" noclick="true" borderthickness="0.5h" color="0 0 0 0.5" x="0.25h" y="0.25h"/>
				<frame texture="/ui/elements:rounded_bg_white" noclick="true" borderthickness="0.5h" color="#191919"/>
				<panel align="right" width="-0.5h" x="-0.25h" y="0.25h" height="2.5h" noclick="true">
					<frame texture="/ui/elements:rounded_bg_white" borderthickness="0.35h" noclick="true" color="#0B0B0B"/>
				</panel>
			
				<button name="gameChatTypeToggleMidgame" interactive="false" style="gameChatTypeSize" noclick="false" x="0.25h" height="2.5h" color="1 1 1 0.1">
					<label name="gameChatTypeLabelMidgame" font="maindyn_12" shadow="true" color="white" textalign="center" noclick="true" content="game_chat_team"/>
				</button>
			</panel>
			<textbox name="gameChatInputBox" style="gameChatInputBox" align="right" width="-7h" onenter="Trigger('GameChatBoxEnter')" onesc="Trigger('GameChatBoxEsc')"/>
		</panel>

		<panel group="gameChatInputContainersEndgame" style="gameChatInputSpace">
		
			<frame texture="/ui/elements:rounded_bg_white" noclick="true" borderthickness="0.5h" color="0 0 0 0.5" x="0.25h" y="0.25h"/>
			<frame texture="/ui/elements:rounded_bg_white" noclick="true" borderthickness="0.5h" color="#191919"/>
			<panel align="right" width="-0.5h" x="-0.25h" y="0.25h" height="2.5h" noclick="true">
				<frame texture="/ui/elements:rounded_bg_white" borderthickness="0.35h" noclick="true" color="#0B0B0B"/>
			</panel>
		
			<button name="gameChatTypeToggleEndgame" interactive="false" style="gameChatTypeSize" noclick="false" x="0.25h" height="2.5h" color="1 1 1 0.1">
				<label name="gameChatTypeLabelEndgame" font="maindyn_12" shadow="true" color="white" textalign="center" noclick="true" content="game_chat_all"/>
			</button>
			
			<textbox name="gameChatInputBoxEndgame" style="gameChatInputBox" align="right" width="-7h" onenter="Trigger('GameChatBoxEnterEndgame')" onesc="Trigger('GameChatBoxEscEndgame')"/>

		</panel>	
		
		<panel group="gameChatInputContainersEndgameLimited" align="right" x="22.0h" y="0.5h" height="100%" width="19h" float="bottom" padding="0.8h" visible="0" >			
			<panel height="2.0h" noclick="1">
				<label textalign="center" style="labelBase" color="1 1 1 .5" outlinecolor="0 0 0 .4" font="subdyn_10" content="end_game_message_header" noclick="1" />
				<instance name="hr" valign="bottom" />
			</panel>			
			<instance id="gameChatInputBtn1" height="3.8h" width="19h" group="endgame_emote_buttons"
				name="standardButton_gameChat"
				label="end_game_message_string_1"
				font="maindyn_20"
				rendermode="normal"
				onclick="AllChat('end_game_message_string_1')"
				onclicklua="self:SetEnabled(0) self:SetRenderMode('grayscale'); GAME_CHAT_EMOTES_USED = GAME_CHAT_EMOTES_USED or 0; GAME_CHAT_EMOTES_USED = GAME_CHAT_EMOTES_USED + 1; if (GAME_CHAT_EMOTES_USED > 1) then local groupTable = self:GetGroup('endgame_emote_buttons') for _, widget in ipairs(groupTable) do widget:SetEnabled(0) end  end"
			/>		
			<instance id="gameChatInputBtn2" height="3.8h" width="19h" group="endgame_emote_buttons"
				name="standardButton_gameChat"
				label="end_game_message_string_2"
				font="maindyn_20"
				rendermode="normal"
				onclick="AllChat('end_game_message_string_2')"
