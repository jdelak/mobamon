					<label name="Dialogue_Message_Text_Title" font="maindyn_20" content="Executioner" color="#aa9d6b" noclick="1"/>
					<image x="-1h" y="2.5h" name="Dialogue_Message_Text_Divider" texture="/ui/game/dialogue/textures/title_underline.tga" color="#aa9d6b" width="50%" height="1h" align="left" noclick="1"/>
					<label y="3.5h" name="Dialogue_Message_Text_Body" font="maindyn_18" content="bastact1_cinematic_executioner1_body" color="1 1 1 0.78" wrap="1" noclick="1"/>
				</panel>
			</panel>
			
