	<!-- Off effects for Dialogue Container -->
	<panel y="-3.5h" width="72h" height="20h" align="center" noclick="1">
		<!-- Smoke no longer needed
		<instance name="Dialogue_Message_Text_SmokeHolder" number="05" direction="LeftOff" x="-68h" y="-18h"/>
		<instance name="Dialogue_Message_Text_SmokeHolder" number="06" direction="LeftOff" x="-58h" y="-18h"/>
		<instance name="Dialogue_Message_Text_SmokeHolder" number="07" direction="LeftOff" x="-48h" y="-18h"/>
		<instance name="Dialogue_Message_Text_SmokeHolder" number="08" direction="LeftOff" x="-38h" y="-18h"/>

		<instance name="Dialogue_Message_Text_SmokeHolder" number="09" direction="RightOff" x="-28h" y="-18h"/>			
		<instance name="Dialogue_Message_Text_SmokeHolder" number="10" direction="RightOff" x="-18h" y="-18h"/>
		<instance name="Dialogue_Message_Text_SmokeHolder" number="11" direction="RightOff" x="-8h" y="-18h"/>
