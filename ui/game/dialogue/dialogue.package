		<instance name="Dialogue_Message_Text_Rubble" number="04" group="Dialogue_Message_Text_Rubble_Off" x="-68h" y="-16h"/>
		<instance name="Dialogue_Message_Text_Rubble" number="05" group="Dialogue_Message_Text_Rubble_Off" x="-45h" y="-16h"/>
	</panel>
	
	<!--
	===========================	
	All Black Backgrounds (TOP)
	===========================
	-->
	<panel name="Cinematic_BlackOverlay" color="black" visible="0" noclick="1"/>
	
	<!--
	===========================	
	Dialogue Messages
	===========================
	-->	
	<panel name="Cinematic_Dialogue_Container" width="95h" height="10h" align="center" valign="top" visible="0" noclick="1">
		<label name="Cinematic_Dialogue_Text" font="maindyn_16" content="bastact1_cinematic_executioner1_body_splitA" color="1 1 1 0.78" textalign="center" textvalign="center" wrap="1" noclick="1"/>
	</panel>
	
	<!--
	===========================	
	End-Game Screen
	===========================
	-->	
	<panel name="Cinematic_EndGame" noclick="0" visible="0" color="0 0 0 0.7">
		<splashimage
			name="Cinematic_EndGame_splash"
			noclick="1"
			visible="1"
			bgcolor="black"
			view="clip"
			onhidelua="self:SetImage('')"
			onshowlua="self:SetImage('/ui/game/loading/textures/bastact1_endscreen.jpg')"
		/>

		<!-- YAY YOU FINISHED SPE CONTENT -->
		<label name="Cinematic_EndGame_act2" x="2h" y="1h" font="maindyn_48" content="origins_act2" color="#fec038" outline="1" outlinecolor="0.53 0.34 0.01 0.22" textalign="center" align="left" valign="top" fitx="1" grow="1" noclick="1" visible="0"/>
		<label name="Cinematic_EndGame_comingsoon" x="2h" y="7h" font="maindyn_30" content="origins_comingSoon" color="1 1 1 1" outline="1" outlinecolor="#4c4c4c" textalign="center" align="left" valign="top" fitx="1" grow="1" noclick="1" visible="0"/>			
		
		<!-- QUEST AND REWARD CONTENT -->
		<panel name="Cinematic_EndGame_questHolder" height="17.3@" width="45%" align="center" valign="center" noclick="1">
			<panel align="center" valign="center" float="right" noclick="1">
				<instance name="Cinematic_EndGame_quest_template" id="Cinematic_EndGame_quest_01" imagetype="resourcesImage" texture="commodity_essence" label="origins_resources" amount="120" imagecolor="0.93 0.58 0.12 0.5" color="#ffd06b" outlinecolor="#573105"/>
				<instance name="Cinematic_EndGame_quest_template" id="Cinematic_EndGame_quest_02" imagetype="resourcesImage" texture="commodity_seal" label="origins_resources" amount="240" imagecolor="0.58 0.12 0.93 0.5" color="#e16bff" outlinecolor="#280557"/>
				<instance name="Cinematic_EndGame_quest_template" id="Cinematic_EndGame_quest_03" imagetype="accountIconImage" texture="spe_act1" label="origins_accountIcon" amount="origins_new" imagecolor="0.12 0.71 0.93 0.7" color="#6bcdff" outlinecolor="#053d57"/>
			</panel>

			<label name="Cinematic_EndGame_questTitle" x="1h" y="-5h" font="maindyn_26" content="origins_questTitle" color="#ffe4f0" outline="1" outlinecolor="#3d2939" textalign="center" align="left" valign="top" fitx="1" grow="1" noclick="1" visible="0"/>
			<label name="Cinematic_EndGame_questComplete" x="1h" y="-8h" font="maindyn_18" content="origins_questComplete" color="#eab8cd" outline="1" outlinecolor="#280822" textalign="center" align="left" valign="top" fitx="1" grow="1" noclick="1" visible="0"/>
		</panel>
		
		<instance
			name="standardButton_endGame" visible="1"
			id="Cinematic_EndGame_survey" y="-12h" font="maindyn_36"
			width="32h" height="8h" align="right" valign="bottom"
			x="-2h" labelY="-1.7h"
			borderthickness="1h"
			label="origins_spe1_survey"
			disabledmouseover="true"
			disabledmouseout="true"
			onclicklua="mainUI.OpenURL(Translate('origins_spe1_survey_url'))"
		/>		
		<label x="-4h" y="-12.8h" font="maindyn_22" content="origins_spe1_survey_desc" color="#ffe4f0" outline="1" outlinecolor="#3d2939" textalign="center" align="right" valign="bottom" width="28h" fity="1" noclick="1" visible="1"/>
		
		<instance
			name="standardButton_endGame" visible="1"
			id="Cinematic_EndGame_close" y="-2h" font="maindyn_36"
			width="32h" height="8h" align="right" valign="bottom"
			x="-2h" labelY="-1.7h"
			borderthickness="1h"
			label="general_close"
			disabledmouseover="true"
			disabledmouseout="true"
			onclicklua="Client.FinishGame()"
		/>
		<label x="-4h" y="-2.8h" font="maindyn_22" content="origins_spe1_close_desc" color="#ffe4f0" outline="1" outlinecolor="#3d2939" textalign="center" align="right" valign="bottom" width="28h" fity="1"  noclick="1" visible="1"/>
		
	</panel>
	
	<lua file="/ui/game/dialogue/dialogue_animations.lua"/>
