	<!--
	===========================	
	Includes / Triggers
	===========================
	-->
	<include file="/ui/game/dialogue/dialogue_templates.package"/>
	
	<trigger name="campaign_startCinematicOverlay"/>
	<trigger name="campaign_endCinematicOverlay"/>
	<trigger name="act1_startCinematicOverlay"/>
	<trigger name="act1_endCinematicOverlay"/>
	<trigger name="cinematic_turnOn"/>
	<trigger name="cinematic_fadeBlackIn"/>
	<trigger name="cinematic_fadeBlackInSlow"/>
	<trigger name="cinematic_fadeBlackOut"/>
