					<instance name="Dialogue_Message_Text_Cracks" number="02" image="02" hflip="1" y="4h" align="right"/>
					<instance name="Dialogue_Message_Text_Cracks" number="03" image="03" hflip="1" y="13.5h" align="right" valign="top"/>
					<instance name="Dialogue_Message_Text_Cracks" number="04" image="04" x="-26h" y="0.1h" valign="top"/>
					<instance name="Dialogue_Message_Text_Cracks" number="05" image="04" x="18h" y="0.1h" valign="top"/>
					<instance name="Dialogue_Message_Text_Cracks" number="06" image="04" vflip="1" x="10h" y="-0.3h" valign="bottom"/>
					<instance name="Dialogue_Message_Text_Cracks" number="07" image="04" vflip="1" x="12h" y="-0.3h" valign="bottom"/>
				</panel>
				
