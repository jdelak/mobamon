			<frame texture="/ui/elements:rounded_corner_white" color="#333333" borderthickness="0.15h"/>
			<frame texture="/ui/elements:roundframe" color="black" borderthickness="0.15h"/>
		</panel>
	</panel>
	<instance name="standardButton" id="gameSpawnLanePusher" wrap="1" visible="0" label="game_lanechoice_button" align="left" height="6.5h" width="18h" x="1.0h" y="22h" valign="top"/>

