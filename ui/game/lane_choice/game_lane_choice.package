								<frame color="#444444" texture="/ui/elements:rounded_bg_white" bordercolor="#444444" borderthickness="0.75h" noclick="true"/>
								<panel hmargin="0.5h" vmargin="0.5h">
									<image name="bossLaneChoiceMidIcon" texture="/ui/game/shared/textures/lane_mid.tga"/>
									<frame color="#444444" bordercolor="#444444" texture="/ui/elements:rounded_corner_white" borderthickness="0.75h" noclick="true"/>
									<frame texture="/ui/elements:roundframe" color="black" borderthickness="0.75h" noclick="true"/>
								</panel>
							</panel>
						</button>
					</panel>
					<panel width="100@" height="100%">
