<?xml version="1.0" encoding="UTF-8"?>
<package>

	<!-- =============================================================== -->
	
	<panel name="laneChoiceTimerContainer" align="left" height="1.5h" visible="0" width="18h" x="1.0h" y="28.8h" valign="top" noclick="true" passivechildren="true">
		<frame color="0 0 0 0.35" texture="/ui/elements:rounded_bg_white" bordercolor="0 0 0 0.5" borderthickness="0.25h" x="0.35h" y="0.35h" noclick="true"/>
		<frame color="#333333" texture="/ui/elements:rounded_bg_white" bordercolor="#333333" borderthickness="0.25h" noclick="true"/>
		<panel hmargin="0.25h" vmargin="0.25h">
			<panel texture="/ui/elements:rounded_bg_white_vert" color="#0B0B0B"/>
