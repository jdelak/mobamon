		<!-- Combat - AltInfoSelfContainer needs to change size to set this properly, or needs it's own y to change -->
		<panel hmargin="4" y="5" valign="bottom" height="2" >
			<?lua
				object:RegisterWatchLua('AltInfoSelf', function(widget, trigger)
					if (trigger.shield > 0) and (trigger.isStunned) then
						widget:SetY('1.7h')
					elseif (trigger.shield > 0) or (trigger.isStunned) then
						widget:SetY('0.9h')
					else
						widget:SetY('5')
					end
				end, true, nil, 'shield', 'isStunned')
			?>

			<panel width="0" color="1 1 0 0.7" visible="0" >
				<?lua
					object:RegisterWatchLua('AltInfoSelf', function(widget, trigger)
						if trigger.inCombat then
							local msCur		= trigger.currentTimeUntilOOC
							local msTotal	= trigger.totalTimeUntilOOC
							local msMod		= math.min(5000, msTotal)
							local msRemain	= msTotal - msCur

							if msRemain <= msMod then
								widget:SetVisible(true)
								widget:SetWidth(ToPercent(
									(msMod - math.min(msMod, msRemain)) / msMod
								))
							else
								widget:SetVisible(false)
							end
						else
							widget:SetVisible(false)
						end
					end, true, nil, 'inCombat', 'currentTimeUntilOOC', 'totalTimeUntilOOC')
				?>
				<panel align="right" color="#78ff00" width="300@" height="100%" />
				<panel align="right" color="1 1 0 1" width="100@" x="-100@" height="100%" />
			</panel>
		</panel>

		<panel hmargin="0" y="1.0h" valign="bottom" height="0.6h" width="-0.6h" x="0" align="center" visible="0">
			<?lua
				object:RegisterWatchLua('AltInfoSelf', function(widget, trigger)
					if (trigger.shield > 0) then
						widget:SetY('1.0h')
					else
						widget:SetY('0.4h')
					end
				end, true, nil, 'shield')
			?>
			<panel width="100%" color="1 0.7 0 1" >
				<?lua
					object:RegisterWatchLua('AltInfoSelf', function(widget, trigger)

						if (trigger.isStunned) then
							widget:GetParent():SetVisible(1)
							widget:SetWidth(ToPercent(trigger.stunnedDurationPercent))
						else
							widget:GetParent():SetVisible(0)
						end
					end, false, nil, 'isStunned', 'stunnedDurationPercent')

				?>
			</panel>
			<panel style="altInfoSelfHealthContainer" texture="$invis" uscale="64p" utile="true">
				<?lua

					object:RegisterWatchLua('AltInfoSelf', function(widget, trigger)

						if (trigger.stunnedMaxDuration > 0) then
							widget:SetUScale(1000 / (trigger.stunnedMaxDuration) )
						end
					end, false, nil, 'stunnedMaxDuration')

				?>
			</panel>
		</panel>

