			<!-- BG 1 -->
			<image width="100%" texture="$white" color="0 0 0 1" visible="0">
				<?lua

					object:RegisterWatchLua('AltInfoSelf', function(widget, trigger)
						widget:SetVisible((trigger.isHovering or trigger.showMoreInfo))
					end, true, nil, 'isHovering', 'showMoreInfo')

				?>
			</image>

			<!-- BG 2 -->
			<image name="AltInfoSelfHealthBase" width="100%" texture="$white" visible="1">
				<?lua
					object:SetColor(styles_healthBarSelfColorBack .. " 0.54")
					object:RegisterWatchLua('updateHealthColors', function(widget)
						widget:SetColor(styles_healthBarSelfColorBack .. " 0.54")
					end)
				?>
			</image>

			<!-- Nameplates lerp in team colours -->
			<image name="AltInfoSelfHealthShadow" width="0%" texture="$white" noclick="1" color="1 0 0 0.65" visible="0"/>

			<!-- Nameplates 'light up' while taking damage to draw attention -->
			<image  width="0%" texture="$white" color="0.0000 1.0000 0.0000 1.0000"  visible="0" >
				<?lua
					object:SetColor(1, 1, 0, 1)

					object:RegisterWatchLua('AltInfoSelf', function(widget, trigger)
						widget:SetVisible(trigger.healthLerp > 0)
						widget:SetWidth(ToPercent(trigger.healthCurrent / (trigger.maxHealth)))
					end, true, nil, 'healthLerp', 'healthCurrent', 'maxHealth')

					object:RegisterWatchLua('AltInfoSelf', function(widget, trigger)
						widget:SetVisible((trigger.isHovering or trigger.showMoreInfo))
					end, true, nil, 'isHovering', 'showMoreInfo')
				?>
			</image>

			<!-- Standard nameplate -->
			<image name="AltInfoSelfHealthBG" width="100%" texture="$white" color="1.0000 1.0000 0.0000 0.7500" visible="1" >
				<?lua
					object:SetColor(styles_healthBarSelfColor)

					object:RegisterWatchLua('AltInfoSelf', function(widget, trigger)
						widget:SetWidth(ToPercent(trigger.healthCurrent / (trigger.maxHealth)))
					end, true, nil, 'healthCurrent', 'maxHealth')

					object:RegisterWatchLua('updateHealthColors', function(widget)
						widget:SetColor(styles_healthBarSelfColor)
					end)
				?>
			</image>

			<panel name="AltInfoSelfHealthPips" style="altInfoSelfHealthContainer" texture="$invis" uscale="64p" utile="true">
				<?lua
					object:RegisterWatchLua('AltInfoSelf', function(widget, trigger)
						local maxHealth = trigger.maxHealth
						if maxHealth > 4500 then
							widget:SetTexture('/ui/game/alt_info/bar_segment_big_8.tga')
						elseif maxHealth >= 3500 then
							widget:SetTexture('/ui/game/alt_info/bar_segment_big_16.tga')
						elseif maxHealth > 1500 then
							widget:SetTexture('/ui/game/alt_info/bar_segment_big_32.tga')
						else
							widget:SetTexture('/ui/game/alt_info/bar_segment_big_128.tga')
						end
						widget:SetUScale(500 / maxHealth)
					end, true, nil, 'maxHealth')
				?>
			</panel>

		</panel>

		<!-- Mana -->
		<panel name="AltInfoSelfMana" hmargin="5@" y="57.7%" height="29.8%">
		
			<!-- BG 1 -->
			<image x="-1" width="+2" texture="$white" color="0 0 0 1" visible="0">
				<?lua

					object:RegisterWatchLua('AltInfoSelf', function(widget, trigger)
						widget:SetVisible((trigger.isHovering or trigger.showMoreInfo))
					end, true, nil, 'isHovering', 'showMoreInfo')

				?>
			</image>
			<panel width="100%" texture="/ui/game/alt_info/textures/mana_pos.tga" color="1 1 1 0.75" >
				<?lua
					object:RegisterWatchLua('AltInfoSelf', function(widget, trigger) widget:SetWidth(ToPercent(trigger.manaPercent)) end, true, nil, 'manaPercent')
					object:RegisterWatchLua('AltInfoSelf', function(widget, trigger)
						if ((trigger.isHovering or trigger.showMoreInfo)) then
							widget:SetColor('1 1 1 1')
						else
							widget:SetColor('1 1 1 0.75')
						end
					end, true, nil, 'isHovering', 'showMoreInfo')
				?>
			</panel>
			<panel width="50%" x="25%" texture="$white" color="0 1 1 0.75" visible="0" >
				<?lua
					object:RegisterWatchLua('AltInfoSelf', function(widget, trigger)
						if (trigger.activeMana > 0) then
							widget:SetVisible(1)
							widget:SetX(ToPercent(trigger.manaPercent - (trigger.activeMana / trigger.maxMana)))
							widget:SetWidth(ToPercent(trigger.activeMana / trigger.maxMana))

							if (trigger.manaCurrent > trigger.activeMana) then
								widget:SetColor('0 1 1 0.75')
							else
								widget:SetColor('1 0.2 0.2 0.75')
							end
						else
							widget:SetVisible(0)
						end
					end, true, nil, 'manaPercent', 'activeMana', 'maxMana', 'manaCurrent')
				?>
			</panel>
		</panel>

