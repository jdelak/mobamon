			<instance name="tempSimpleCheckboxState" bg_color="{bg_color=0 0 0 .8}" checkvisible="true" statename="over2"/>
			<instance name="tempSimpleCheckboxState" bg_color="{bg_color=0 0 0 .8}" checkvisible="true" statename="down2" x="1" y="1"/>
			<instance name="tempSimpleCheckboxState" bg_color="{bg_color=0 0 0 .8}" checkvisible="true" statename="disabled2"/>
