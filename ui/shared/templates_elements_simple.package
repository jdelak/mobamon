<?xml version="1.0" encoding="UTF-8"?>
<package>

	<template name="deckTab">
		<button name="{id}deckTabButton" grow="1" regrow="1" growinvis="0" height="64s" sticky="1" stickytoinvis="0" passivechildren="true" color="invisible"
			onclicklua="
				{onclicklua}
			"
			onmouseldownlua="
				if ('{defaultsound=1}' == '1') then
					PlaySound('/ui/sounds/shop/sfx_category_select.wav', 1.0)
				end
			"			
			onmouseoverlua="GetWidget('{id}deckTabOffHover'):FadeIn(90)"
			onmouseoutlua="GetWidget('{id}deckTabOffHover'):FadeOut(90)"			
		>
		
			<panel name="{id}deckTabOff" group="deckTabsOff" height="64s" grow="1" clip="1" noclick="1" visible="{isOff=0}">
				<image texture="/ui/main/shared/textures/tab_off_c.tga" width="-32s" height="64s" align="center" noclick="1"/>
				<image texture="/ui/main/shared/textures/tab_off_l.tga" width="16s" height="64s" align="left" noclick="1"/>
				<image texture="/ui/main/shared/textures/tab_off_R.tga" width="16s" height="64s" align="right" noclick="1"/>				
				<image texture="/ui/main/shared/textures/tab_off_bottom.tga" width="-32s" height="64s" align="center" color="1 1 1 .2" noclick="1"/>
				<panel name="{id}deckTabOffHover" noclick="1" visible="0">
					<image texture="/ui/main/shared/textures/tab_off_c.tga" width="-32s" height="64s" align="center" noclick="1" rendermode="additive" color="1 1 1 .3" />
					<image texture="/ui/main/shared/textures/tab_off_l.tga" width="16s" height="64s" align="left" noclick="1" rendermode="additive" color="1 1 1 .3"/>
					<image texture="/ui/main/shared/textures/tab_off_R.tga" width="16s" height="64s" align="right" noclick="1" rendermode="additive" color="1 1 1 .3"/>
				</panel>
				<panel noclick="1" x="22s" y="10s" grow="1" regrow="1" growinvis="0" float="right">
					<label font="maindyn_26" content="{label}" fitx="1" fitxpadding="6s" color="#aacee8" outline="1" outlinecolor="#003052" noclick="1" sticky="1" stickytoinvis="0" />					
					<panel grow="1" regrow="1" growinvis="0" noclick="1">
						<frame name="{id}deckTabButtonTab" texture="/ui/shared/frames/key.tga" y="2s" height="28s" borderthickness="6s" width="48s" color="#acd7f5" bordercolor="#acd7f5" noclick="1" visible="0">
							<label y="-2s" font="maindyn_20" style="labelBase" outlinecolor="#d0dfea" color="#19527b" textalign="center" textvalign="center" content="decks_tab" noclick="1" />
						</frame>
					</panel>
					<panel noclick="1" width="10s" sticky="1" stickytoinvis="0" />
				</panel>
			</panel>
			<panel name="{id}deckTabOn" group="deckTabsCurrent" height="64s" grow="1" clip="1" noclick="1" visible="{isCurrent=0}">
				<image texture="/ui/main/shared/textures/tab_on_c.tga" width="-32s" height="64s" align="center" noclick="1"/>
				<image texture="/ui/main/shared/textures/tab_on_l.tga" width="16s" height="64s" align="left" noclick="1"/>
				<image texture="/ui/main/shared/textures/tab_on_R.tga" width="16s" height="64s" align="right" noclick="1"/>		
				<image texture="/ui/main/shared/textures/tab_on_bottom.tga" width="-32s" height="64s" align="center" color="1 1 1 1" noclick="1"/>								
				<image texture="/ui/main/shared/textures/tab_on_arrow.tga" width="16s" height="64s" align="center" noclick="1"/>
				<label font="maindyn_26" x="22s" y="10s" content="{label}" fitx="1" fitxpadding="22s" color="#f2f8ff" outline="1" outlinecolor="#1a57a8" noclick="1" />
			</panel>	
		</button>
	</template>

	<template name="NumberValue">
		<panel name="{id}" grow="true" regrow="true" x="{x}" y="{y}" height="{height=22s}" align="{align}" valign="{valign}" visible="{countShown=1}" noclick="true" passivechildren="true">
			<frame texture="/ui/elements:rounded_bg_white" borderthickness="4s" color="0 0 0 .8"/>
			<frame texture="/ui/elements:roundframe" borderthickness="4s" color=".9 .9 .9 1"/>
			<label name="{labelId}" x="6s" fitx="true" fitxpadding="6s" height="100%" font="{font=maindyn_18}" color="1 1 1 1" outline="true" outlinecolor="0 0 0 .5" content="{label}" />
		</panel>
	</template>
	
	<template name="DropTarget">
		<panel name="{index}_parent" width="{width=+10s}" x="{x}" y="{y}"	height="{height=+10s}" align="center" valign="center" noclick="1" visible="{visible=false}" >
			<frame texture="/ui/shared/frames/rounded_bg_solid.tga" borderthickness="{borderthickness=6s}" color="{color=#0cb9e5}2d" noclick="1"/>
			<frame texture="/ui/shared/frames/border_double.tga" borderthickness="{borderthickness=6s}" bordercolor="{color=#0cb9e5}" color="invisible" noclick="1"/>
			<droptarget name="{index}_droptarget" />
			<label content="{label}" x="8s" y="-2s" fity="1" outline="1" outlinecolor="black" fitx="1" font="maindyn_20" align="left" textalign="left" valign="center" noclick="1" color="#0cb9e5" />
		</panel>
	</template>	
	
	<template name="DropLand">
		<panel name="{id}" width="{width=+10s}" x="{x}" y="{y}"	height="{width=+10s}" align="center" valign="center" noclick="{noclick=0}" visible="{visible=false}">
			<frame texture="/ui/shared/frames/rounded_bg_solid.tga" borderthickness="{borderthickness=6s}" color="{color=#0cb9e5}2d" noclick="1"/>
			<frame texture="/ui/shared/frames/border_double.tga" borderthickness="{borderthickness=6s}" bordercolor="{color=#0cb9e5}" color="invisible" noclick="1"/>
		</panel>
	</template>

	<template name="simpleDropdownItem">
		<label x="6s" width="-45s" style="labelBase" color="{fontColor=1 1 1 1}" font="{font=maindyn_18}" textvalign="center" content="{label}"/>
	</template>
	<template name="simpleDropdownItem3Part">
		<label x="6s" width="35%" style="labelBase" color="{fontColor=1 1 1 1}" font="{font=maindyn_18}" textvalign="center" content="{label1}"/>
		<label x="48.5%" width="25%" style="labelBase" color="{fontColor=1 1 1 1}" font="{font=maindyn_18}" textvalign="center" content="{label2}"/>
		<label x="76%" width="25%" style="labelBase" color="{fontColor=1 1 1 1}" font="{font=maindyn_18}" textvalign="center" content="{label3}"/>
	</template>	
	
	<template name="simpleDropdownItem2Labels">
		<label x="2s" fitx="1" fity="1" align="left" style="whiteTextColor" shadow="true" shadowoffset="2" font="maindyn_18" textvalign="center" content="{label1}"/>
		<label x="-2s" fitx="1" fity="1" align="right" style="whiteTextColor" shadow="true" shadowoffset="2" font="maindyn_18" textvalign="center" content="{label2}"/>
	</template>	
	
	<template name="simpleDropdownItemIcon">
		<image texture="{icon=/ui/main/shared/textures/user_status_light.tga}" color="{iconcolor=1 1 1 1}" y="-1@" x="4s" width="53@" height="53%" valign="center"/>
		<label y="-1s" x="92@" width="-10s" style="labelBase" color="{fontColor=1 1 1 1}" font="{font=maindyn_18}" textvalign="center" content="{label}"/>
	</template>

	<template name="simpleCombobox">
		<combobox
			width="{width=100%}"
			height="{height=100%}"
			align="{align}"
			valign="{valign}"
			color="invisible"
			itemheight="3.0h"
			texture="{texture}"
			scrolltexture="/ui/shared/textures/slider.tga"
			handleheight="48s"
			scrollbarsize="24s"
			highlight="over"
			highlightcolor="invisible"
			highlightbordercolor="invisible"
			maxlistheight="{maxlistheight=5}"
			onloadlua="{onloadlua}"
			onselectlua="{onselectlua}"
			cvar="{cvar}"
			name="{id}"
			x="{x}"
			y="{y}"
			enabled="{enabled=true}"
			visible="{visible=1}"
			interactive="{interactive=false}"
		>
			<widgetstate statename="up">
				<panel>
					<image width="16@" texture="/ui/shared/textures/combobox_{isBlue}background_l.tga" color=".8 .8 .8 1" />
					<image x="16@" width="-116@" texture="/ui/shared/textures/combobox_{isBlue}background_c.tga" color=".8 .8 .8 1" />
					<image width="100@" align="right" texture="/ui/shared/textures/combobox_{isBlue}background_r.tga" color=".8 .8 .8 1" />
				</panel>
			</widgetstate>
			<widgetstate statename="over">
				<panel>
					<image width="16@" texture="/ui/shared/textures/combobox_{isBlue}background_l.tga" color="1 1 1 1" />
					<image x="16@" width="-116@" texture="/ui/shared/textures/combobox_{isBlue}background_c.tga" color="1 1 1 1" />
					<image width="100@" align="right" texture="/ui/shared/textures/combobox_{isBlue}background_r.tga" color="1 1 1 1" />
				</panel>
			</widgetstate>
			<widgetstate statename="disabled">
				<panel>
					<image width="16@" texture="/ui/shared/textures/combobox_{isBlue}background_l.tga" color=".4 .4 .4 1" />
					<image x="16@" width="-116@" texture="/ui/shared/textures/combobox_{isBlue}background_c.tga" color=".4 .4 .4 1" />
					<image width="100@" align="right" texture="/ui/shared/textures/combobox_{isBlue}background_r.tga" color=".4 .4 .4 1" />
				</panel>
			</widgetstate>
			<widgetstate statename="itembg">
			</widgetstate>
			<widgetstate statename="listbg">				
				<panel y="-0.8h" height="+1.6h" width="-0.4h" align="center" >
					<frame texture="/ui/elements:rounded_bg_white" borderthickness="0.6h" color="0 0 0 .9" noclick="true"/>
					<frame texture="/ui/elements:roundframe" borderthickness="0.6h" color=".2 .2 .2 1" noclick="true" />					
				</panel>			
			</widgetstate>
			<widgetstate statename="itemhighlight"> 
				<panel x="1.0h" width="-3.5h">
					<frame texture="/ui/elements:rounded_bg_white" borderthickness="0.4h" color="1 1 1 .15" noclick="true"/>
				</panel>
			</widgetstate>
		</combobox>
	</template>

