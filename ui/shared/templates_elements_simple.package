	<template name="tempSimpleCheckboxState">
		<widgetstate statename="{statename}" x="{x}" y="{y}">
			<frame texture="/ui/shared/frames/rounded_bg_solid.tga" borderthickness="{borderthickness=6s}" color="{bg_color}" noclick="1"/>
			<frame texture="/ui/shared/frames/border_double.tga" borderthickness="{borderthickness=6s}" bordercolor=".4 .4 .4 1" color="invisible" noclick="1"/>
			<image texture="/ui/main/shared/textures/close.tga" width="70@" height="70%" align="center" valign="center" color=".8 .8 .8 1" visible="{checkvisible=false}"/>
		</widgetstate>
	</template>

	<template name="tempSimpleCheckbox">
		<button name="{id}" width="{width=100%}" height="{height=100%}" x="{y}" y="{y}" align="{align}" valign="{valign}" color="{color=invisible}" numstates="2" cvar="{cvar}" interactive="false">
			<instance name="tempSimpleCheckboxState" bg_color="{bg_color=0 0 0 .8}" statename="up"/>
			<instance name="tempSimpleCheckboxState" bg_color="{bg_color=0 0 0 .8}" statename="over" />
