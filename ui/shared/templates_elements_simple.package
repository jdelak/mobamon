				<frame name="{buttonName}Backer" texture="/ui/shared/frames/stnd_btn_up.tga" borderthickness="{borderthickness=0.6h}" />
				<image align="center" valign="center" width="70@" height="70%" texture="/ui/main/shared/textures/close.tga" />
			</panel>
		</button>
	</template>

