	<template name="craftingItemBonusLineEntry">
		<panel name="{id}Container" grow="true" regrow="true" width="100%" sticky="true" stickytoinvis="false" visible="{visible=false}">
			<panel>
				<frame style="backerPanelShadow0" texture="/ui/elements:rounded_bg_white" borderthickness="0.75h" noclick="true" x="0.25h" y="0.25h"/>
				<frame style="backerPanelGray2" texture="/ui/elements:rounded_bg_white" borderthickness="0.75h" noclick="true"/>
			</panel>
			<panel align="center" width="-0.75h" grow="true" regrow="true" float="bottom" padding="0.5h">
				<panel height="0"/>
				<panel height="2.75h">
					<panel width="100@" height="100%">
						<image name="{id}Icon" texture="{icon=/heroes/harrower/ability_01/icon.tga}"/>
						<frame style="backerPanelGray2" texture="/ui/elements:rounded_corner_white" borderthickness="0.75h"/>
						<frame color="black" texture="/ui/elements:roundframe" borderthickness="0.75h"/>
					</panel>
					<label name="{id}Title" align="right" width="-3.5h" textvalign="center" font="maindyn_14" color="{titleColor=#f5f2e2}" shadow="true" content="{title=}"/>
				</panel>
				<panel grow="true" width="100%">
					<panel>
						<frame style="backerPanelGray0" texture="/ui/elements:rounded_bg_white" borderthickness="0.75h"/>
						<frame style="backerPanelBorder0Dark" texture="/ui/elements:roundframe" borderthickness="0.75h"/>
						<frame style="backerPanelBorder0Light" texture="/ui/elements:roundframe" borderthickness="0.75h" width="+0.1h" height="+0.1h"/>
					</panel>
					<label name="{id}Description" width="-1h" textvalign="center" align="center" fity="true" fitypadding="1h" style="whiteTextColor" font="maindyn_12" wrap="true" content="{description=}"/>
				</panel>
				<panel height="0"/>
			</panel>
		</panel>
	</template>
	
	<template name="standardButton2Check">
		<button name="{id}" width="{width=100%}" height="{height=100%}" x="{x}" y="{y}" align="{align}" valign="{valign}" passivechildren="true" visible="{visible=true}" sticky="{sticky}" stickytoinvis="{stickytoinvis=true}" interactive="{interactive=false}" enabled="{enabled=true}" disabledmouseover="{disabledmouseover=false}" disabledmouseout="{disabledmouseout=false}" noclick="{noclick=false}" color="invisible" onclick="{onclick}" onclicklua="{onclicklua}" numstates="2"
			onloadlua="
				local disableCheck = '{disableCheck=false}'
				local rendermode = '{rendermode=normal}'
				local infoTable = libGeneral.newTable()
				infoTable.disableCheck  = AtoB(disableCheck)
				infoTable.rendermode  	= rendermode
				libButton2.initializeButton(
					self, '{id}', 'standardButton2Check', infoTable
				)
				{onloadlua}
				if Empty('{id}') then
					FindUnnamedWidget(self, 'standardButton2Check')
				end	
