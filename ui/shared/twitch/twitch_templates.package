			<panel group="twitch_helper_{index}_resize_constrain" visible="{canResizeConstrained=0}" noclick="1" color="1 1 0 1" width="5@" height="5%" align="left" valign="bottom" />
			<panel group="twitch_helper_{index}_resize_constrain" visible="{canResizeConstrained=0}" noclick="1" color="1 1 0 1" width="5@" height="5%" align="right" valign="top" />
			<panel group="twitch_helper_{index}_resize_constrain" visible="{canResizeConstrained=0}" noclick="1" color="1 1 0 1" width="5@" height="5%" align="right" valign="bottom" />	
			
			<panel group="twitch_helper_{index}_resize" visible="{canResize=0}" noclick="1" color="1 1 0 1" width="5@" height="5%" align="right" valign="center" />
