	<template name="twitch_helper_template_nosize" >
		<panel name="twitch_helper_{index}" group="twitch_helpers{temp}" visible="1" align="left" height="20h" width="20h" x="0" y="0" valign="top" color="1 1 0 0.1" noclick="0" constraintoparent="1"
