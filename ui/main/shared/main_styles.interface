		color="invisible"
		exteriorscrollbars="true"
		itemheight="4.3h"
		scrollbaroffset="0.75h"
		scrollbarsize="3h"
		handlewidth="3h"
		handleheight="6h"
		scrollbarplaceholder="true"
		scrolltexture="/ui/shared/textures/slider.tga"
		highlightcolor="invisible"
		hscrollbar="0"
		horizontal="false"		
	/>
	
