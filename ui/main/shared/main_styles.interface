	<style name="overTransparentLightOutline" outline="true" outlinecolor="0 0 0 0.5"/>
	
	<style name="lightTanBrownOutlineText" color="#fae9c9" outline="true" outlinecolor="#4c3e30"/>
	
	<style name="mainPanelHeaderSmall" textvalign="center" noclick="true" font="maindyn_20" outline="true" outlinecolor="0 0 0 0.5" color="white" height="3.5h" y="0.25h" x="1.5h"/>

	<style name="generalProgressBarColor" color="#991814"/>
	
