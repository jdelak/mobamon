<?xml version="1.0" encoding="UTF-8"?>
<package>

	<template name="demoNav_playScreenOption">
		<panel name="demoNav_playScreenOption{id}Parent" group="playScreenOptionParents" width="300s" height="100%" sticky="{sticky=true}" stickytoinvis="false" visible="{visible=1}">					
			<frame texture="/ui/elements:squareglow" color="0 0 0 .4" borderthickness="20s" width="+28s" height="+28s" align="center" valign="center" />
			<frame name="demoNav_playScreenOption{id}BackGlow" texture="/ui/elements:squareglow" color=".3 .91 .98 .2" borderthickness="16s" rendermode="additive" width="+24s" height="+24s" align="center" valign="center" visible="0" noclick="1">
				<modelpanel x="-5s" height="+140s" width="+480s" align="center" valign="center" visible="1"
					noclick="true"
					model="/shared/models/invis.mdf"
					effect="/ui/main/play/effects/mode.effect"
					onshowlua="self:SetEffect('/ui/main/play/effects/mode.effect')"
					onhidelua="self:SetEffect('')"
					camerapos="0 0 0"
					cameraangles="0 90 0"
					camerafov="86"
					depthclear="true"
					depthcompress="true"
				/>
			</frame>			
			<image texture="/ui/shared/textures/stone_backer/tall_base_top.tga" color="{basecolor=#82e5fa}" height="12.5@" noclick="1"/>
			<image texture="/ui/shared/textures/stone_backer/tall_base_bottom.tga" color="{basecolor=#82e5fa}" height="12.5@" valign="bottom" noclick="1"/>
			<image texture="/ui/shared/textures/stone_backer/tall_base_center.tga" color="{basecolor=#82e5fa}" vmargin="12.5@" noclick="1"/>
			
			<image texture="/ui/shared/textures/stone_backer/crack_type02.tga" hflip="true" width="14%" height="14@" align="right" valign="bottom" x="-8@" y="-0.4%" rotation="90" noclick="1"/>
			<image texture="/ui/shared/textures/stone_backer/crack_type03.tga" hflip="true" width="8%" height="8@" align="right" valign="bottom" x="-3.4@" y="-0.2%" rotation="90" noclick="1"/>
			<image texture="/ui/shared/textures/stone_backer/crack_type05.tga" hflip="true" width="10%" height="10@" align="left" valign="top" x="3@" y="0.5%" rotation="-90" noclick="1"/>
			<image texture="/ui/shared/textures/stone_backer/crack_type04.tga" hflip="true" width="11%" height="11@" align="left" valign="bottom" x="4@" y="-0.6%" vflip="1" noclick="1"/>
			
			<panel name="demoNav_playScreenOption{id}Highlight" visible="1" noclick="1">
				<image color="{glowcolor=#0d7daa}" rendermode="additive" texture="/ui/shared/textures/stone_backer/tall_glow_top.tga" height="12.5@" noclick="1"/>
				<image color="{glowcolor=#0d7daa}" rendermode="additive" texture="/ui/shared/textures/stone_backer/tall_glow_bottom.tga" height="12.5@" valign="bottom" noclick="1"/>
				<image color="{glowcolor=#0d7daa}" rendermode="additive" texture="/ui/shared/textures/stone_backer/tall_glow_center.tga" vmargin="12.5@" noclick="1"/>
			</panel>
					
			<panel name="demoNav_playScreenOption{id}Clipper" y="-40s" height="104%" width="-20s" align="center" clip="0" noclick="1">
				<image name="demoNav_playScreenOption{id}Art" texture="" onshowlua="self:SetTexture('/ui/main/background/textures/{artTexture}')" y="{artY}" x="{artX}" width="512s" height="512s" align="center" valign="bottom" color="1 1 1 1" nocompress="true" noclick="1" />
			</panel>

			<panel name="demoNav_playScreenOption{id}TitleContainer" width="-30s" height="-16s" valign="center" align="center" noclick="1" visible="1">
				<label font="{title_font=maindyn_36}" style="labelBase" color="{title_color}" y="2s" textalign="center" content="{title}" hmargin="{title_hmargin}" wrap="1" lineheight="32s" noclick="1" />				
				<label font="subdyn_13" style="labelBase" color="#b9f9ff" textalign="center" y="79s" wrap="1" lineheight="18s" content="{details}" hmargin="{details_hmargin}" noclick="1" />
			</panel>
			
			<panel y="70s" align="center" valign="bottom" grow="1" growinvis="0" height="56s">
			
				<instance name="standardButton"
					id="demoNav_playScreenOption{id}Button" 
					width="{btn_width=270s}" valign="bottom" borderthickness="10s"
					label="{btn_label}" font="maindyn_30"
				/>
				
				<instance name="standardButton"
					id="demoNav_playScreenOption{id2}Button" 
					visible="{hasbuttonSet2=0}"
					x="156s"
					width="{btn2_width=140s}" valign="bottom" borderthickness="10s"
					label="{btn2_label}" font="maindyn_30"
				/>
			
			</panel>
			
		</panel>
	</template>

