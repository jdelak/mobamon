				<instance name="demoNav_playScreenOption" id="3" artTexture="pax_play_3.tga" artY="0" 	artX="12s" 	title="main_demo_button_3_title"	title_hmargin="10s"		title_color="#f4a837"	btn_label="main_demo_button_3_btn_title" 		details="main_demo_button_3_btn_desc"	details_hmargin="0" 	buttonSet="playScreenOptionsCustomMatches" 	visible="1" />
				<instance name="demoNav_playScreenOption" id="4" artTexture="pax_play_4.tga" artY="70s" 	artX="4s" 	title="main_demo_button_4_title"	title_font="maindyn_40"	title_hmargin="10s"		title_color="#9e41b6"	btn_label="main_demo_button_4_btn_title" 		details="main_demo_button_4_btn_desc"	details_hmargin="30s" 	buttonSet="playScreenOptionsCustomMatches" 	visible="1" />
			</panel>		
		</panel>
		
		<panel name="main_demo_ui_signup_parent" group="demo_navigation_animation_widgets" x="55s" y="90s" width="470s" height="128s" align="left" valign="top" noclick="0" visible="1">
			<instance name="dialogWrapper1" />
			<panel grow="1" y="0" width="-20s" align="center" valign="center" float="bottom" padding="12s" noclick="1">
				<label fity="1" textalign="center" style="labelBase" font="maindyn_20" lineheight="22s" wrap="1" color=".7 .7 .7 1" content="" onloadlua="self:SetText(FormatStringNewline(Translate('main_demo_signup_desc')))" noclick="1"/>								
				<panel height="40s" noclick="1">									
