				<panel width="1" align="left" color="0 0 0 .2" noclick="1" />
				<panel width="1" align="right" color="1 1 1 .2" noclick="1" />
			</panel>
		</panel>
	</template>

	<template name="mainCraftingDraggableComponent">
		<panel width="94s" height="94s" group="crafting_animation_widgets" >
			<button name="craftingDraggableComponent{type}{id}" group="craftingDraggableComponents" color="invisible" cangrab="true" dragexclusive="false">
				<frame noclick="true" texture="/ui/elements:squareglow" color="0 0 0 .7" width="+12s" height="+12s" align="center" valign="center" borderthickness="10s" />
				<frame name="craftingDraggableComponent{type}{id}hoverGlow" texture="/ui/shared/frames/frameglow.tga" y="-6s" width="+12s" height="80%" borderthickness="16s" color=".4 .9 1 .5" align="center" valign="top" rendermode="additive" noclick="1" visible="0" />
				<frame group="craftingDraggableComponent_alert_glows" visible="0" noclick="true" texture="/ui/shared/frames/squareglow.tga" borderthickness="10s" color="invisible" width="+12s" height="+12s" align="center" valign="center" bordercolor=".4 .9 1 .5">
					<modelpanel height="+28s" width="+28s" align="center" valign="center" visible="1"
						noclick="true"
						model="/shared/models/invis.mdf"
						effect="/ui/main/crafting/effects/alert.effect"
						onshowlua="self:SetEffect('/ui/main/crafting/effects/alert.effect')"
						onhidelua="self:SetEffect('')"
						camerapos="0 0 0"
						cameraangles="0 0 0"
						camerafov="40"
						depthclear="true"
						depthcompress="true"
					/>
				</frame>
				<panel width="77.3@" height="77.3%" align="center" valign="center" color="0 0 0 0.65">
					<image name="craftingDraggableComponent{type}{id}Icon" texture="/ui/shared/textures/pack2.tga"/>
					<frame texture="/ui/shared/frames/inner_glow.tga" borderthickness="14s" bordercolor="0 0 0 .8" align="center" noclick="1" />
				</panel>
				<image name="craftingDraggableComponent{type}{id}Frame" texture="/ui/main/shared/textures/iconframe_interactive2.tga" noclick="1" />
				<image name="craftingDraggableComponent{type}{id}hoverFrame" texture="/ui/main/shared/textures/iconframe_interactive2_hover.tga" visible="0" noclick="1" />
				<panel height="34s" width="34s" x="-6s" y="5s" align="left" valign="bottom" visible="1" noclick="true" passivechildren="true">
					<image name="craftingDraggableComponent{type}{id}JublieImage" group="craftingDraggableComponentJublies" height="100%" width="100@" texture="/ui/main/crafting/textures/jublie.tga" color=".5 .5 .5 1" noclick="1" />
					<label name="craftingDraggableComponent{type}{id}Cost" y="-1s" textalign="center" textvalign="center" style="labelBase" font="maindyn_20" color="1 1 1 1" content="" />
				</panel>
			</button>
			<button name="craftingDraggableComponent{type}{id}SelectButton" color="invisible">
				<instance name="DropLand" width="100@" height="100%" visible="1"/>
			</button>
			<panel name="craftingDraggableComponent{type}{id}ClickDropTarget" visible="false"/>
		</panel>
	</template>

	<template name="mainCraftingDraggableComponentRow">
		<panel grow="1" noclick="true">
			<panel y="29s" height="12.5h" float="right" padding="8s" noclick="true">
