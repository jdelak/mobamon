<?xml version="1.0" encoding="UTF-8"?>
<package>

	<template name="crafting_purchase_option">
		<panel noclick="0" width="100%" height="46s" >			
			<frame noclick="1" texture="/ui/shared/frames/rounded_bg_solid.tga" borderthickness="6s" color=".1 .1 .1 1" />
			<frame noclick="1" texture="/ui/shared/frames/border_double.tga" borderthickness="6s" bordercolor=".2 .2 .2 1" color="invisible"/>			
			<label x="10s" noclick="1" textvalign="center" style="h3" font="maindyn_26" content="{label=Permanent}" />			
			<panel x="-4s" grow="1" noclick="1" height="40s" align="right" valign="center" float="right" padding="2s">
				<label noclick="1" name="crafting_prompt_purchase_craft_elixir_cost" fitx="true" textvalign="center" font="maindyn_26" style="labelBase" content="{cost=666}" />
				<panel noclick="1" width="100@" height="100%">
					<image noclick="1" texture="{icon=/ui/main/shared/textures/commodity_essence.tga}" width="94@" height="94%" align="center" valign="center"/>
				</panel>
			</panel>			
		</panel>
	</template>
	
	<template name="mainCraftingDraggableImbuement">
		<panel width="49%" height="100%" noclick="true" group="crafting_animation_widgets">
			<button name="craftingImbuement{id}" group="craftingImbuements" color="invisible" cangrab="true" dragexclusive="false">
				<panel width="484s" height="254s">
					<panel width="268s" height="142s" name="craftingImbuement{id}_dummyglows"  group="craftingImbuement_alert_glows" noclick="1" >
						<frame visible="1" noclick="true" texture="/ui/shared/frames/frameglow.tga" color=".4 .9 1 .7" bordercolor=".4 .9 1 .7" rendermode="additive" width="+20s" height="+14s" align="center" valign="center" borderthickness="16s" />
						<frame visible="1" noclick="true" color="invisible" bordercolor="invisible" width="+12s" height="+40s" align="center" valign="center">
							<modelpanel height="+82s" width="+124s" align="center" valign="center" visible="1"
								noclick="true"
								model="/shared/models/invis.mdf"
								effect="/ui/main/crafting/effects/alert_wide.effect"
								onshowlua="self:SetEffect('/ui/main/crafting/effects/alert_wide.effect')"
								onhidelua="self:SetEffect('')"
								camerapos="0 0 0"
								cameraangles="0 0 0"
								camerafov="65"
								depthclear="true"
								depthcompress="true"
							/>
						</frame>
					</panel>				
					<frame name="craftingImbuement{id}hoverFrame" texture="/ui/shared/frames/frameglow.tga" x="-4s" y="-1s" width="282s" height="120s" borderthickness="18s" color=".4 .9 1 .5" rendermode="additive" noclick="1" visible="1" />
					<image name="craftingImbuement{id}Frame" texture="/ui/main/crafting/textures/imbue_bg_up.tga" />
					<image name="craftingImbuement{id}hoverGlow" texture="/ui/main/crafting/textures/imbue_bg_hover.tga" visible="0"/>						
				</panel>
				<panel hmargin="22s" vmargin="7s" noclick="1">
					<panel y="-11s" x="-30s" width="64s" height="64s" noclick="1" visible="1">
						<image color="0 0 0 .4" x="-3s" y="3s" texture="/ui/main/crafting/textures/imbue_icon.tga"/>
						<image name="craftingImbuement{id}Icon" texture="/ui/main/crafting/textures/imbue_icon.tga"/>
					</panel>
					<panel width="-38s" align="right">
						<label name="craftingImbuement{id}Name" style="labelBase" color="#b712d6" font="maindyn_22" content="--"/>
						<panel y="26s" grow="1" height="20s" float="right" padding="2s" noclick="1">
							<label name="craftingImbuement{id}Cost" fitx="true" textvalign="center" style="labelBase" font="maindyn_22" content="+--" noclick="1"/>
							<image height="100%" width="100@" texture="/ui/elements:gold_coins" noclick="1" />
						</panel>
					</panel>					
					<label name="craftingImbuement{id}Desc" y="61s" wrap="1" style="labelBase" color=".7 .7 .7 1" font="subdyn_13" content="--"/>
				</panel>
			</button>
		</panel>
	</template>

	<template name="mainCraftingComponentCostPip">
		<panel width="6.667%"  noclick="1">
			<label name="mainCraftingComponentCostPip{id}Label" textalign="center" textvalign="center" style="labelBase" color="#666666" font="maindyn_24" content="{id}"  noclick="1"/>
