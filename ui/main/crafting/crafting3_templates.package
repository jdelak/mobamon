				<instance name="mainCraftingDraggableComponent" id="2" type="{type1}"/>
				<instance name="mainCraftingDraggableComponent" id="3" type="{type1}"/>
				<panel width="18s" noclick="true"/>
				<instance name="mainCraftingDraggableComponent" id="1" type="{type2}"/>
				<instance name="mainCraftingDraggableComponent" id="2" type="{type2}"/>
			</panel>
			<panel y="0" height="24s" noclick="true">
				<label x="8s" textvalign="center" color="white" font="maindyn_22" outline="true" noclick="true" content="{type1}" group="crafting_animation_widgets"/>
				<label x="338s" textvalign="center" color="white" font="maindyn_22" outline="true" noclick="true" content="{type2}" group="crafting_animation_widgets"/>
			</panel>
		</panel>
	</template>

	<template name="mainCraftingNewItemComponent">
		<panel name="craftingNewItemComponent{id}" width="188s" height="100%" x="{x}" y="{y}" valign="{valign}" XXXgroup="crafting_animation_widgets">
			<frame name="craftingNewItemComponent{id}Hover" texture="/ui/elements:squareglow" borderthickness="8s" width="+10s" height="+10s" align="center" valign="center" bordercolor=".4 .9 1 .4" color=".4 .9 1 .4" rendermode="additive" visible="0"/>
			<panel name="craftingNewItemComponent{id}_dummyglows" noclick="1" >
				<frame name="craftingNewItemComponent{id}_dummyglow_2" visible="0" noclick="true" texture="/ui/shared/frames/frameglow.tga" color=".4 .9 1 .7" bordercolor=".4 .9 1 .7" rendermode="additive" width="+12s" height="+10s" align="center" valign="center" borderthickness="16s" />
				<frame name="craftingNewItemComponent{id}_dummyglow_1" visible="0" noclick="true" color="invisible" bordercolor="invisible" width="+12s" height="+40s" align="center" valign="center">
					<modelpanel height="+12s" width="+24s" align="center" valign="center" visible="1"
						noclick="true"
						model="/shared/models/invis.mdf"
						effect="/ui/main/crafting/effects/alert_wide.effect"
						onshowlua="self:SetEffect('/ui/main/crafting/effects/alert_wide.effect')"
						onhidelua="self:SetEffect('')"
						camerapos="0 0 0"
						cameraangles="0 0 0"
						camerafov="65"
						depthclear="true"
						depthcompress="true"
					/>
				</frame>
			</panel>
			<frame name="craftingNewItemComponent{id}_alert_glow" visible="0" noclick="true" texture="/ui/shared/frames/frameglow.tga" color=".9 .7 0 .2" bordercolor=".9 .7 0 .2" y="5s" borderthickness="16s" width="+12s" height="+20s" align="center" valign="center">
				<modelpanel y="-6s" height="+26s" width="+20s" align="center" valign="center" visible="1"
					noclick="true"
					model="/shared/models/invis.mdf"
					effect="/ui/main/crafting/effects/alert_red.effect"
					onshowlua="self:SetEffect('/ui/main/crafting/effects/alert_red.effect')"
					onhidelua="self:SetEffect('')"
					camerapos="0 0 0"
					cameraangles="0 0 0"
					camerafov="74"
					depthclear="true"
					depthcompress="true"
				/>
			</frame>
			<frame texture="/ui/shared/frames/std_btn_inset.tga" borderthickness="6s" bordercolor=".9 .9 .9 1" color=".9 .9 .9 1" />
			<button name="craftingNewItemComponent{id}SelectComponentButton" color="invisible">
				<panel name="craftingNewItemComponent{id}Body" x="6@" width="86@" height="86%" valign="center">
					<image name="craftingNewItemComponent{id}Icon" />
					<instance name="iconFrame" />
				</panel>
				<panel y="16s" align="right" width="-95@" float="bottom" noclick="1">
					<label name="craftingNewItemComponent{id}Name" fity="1" textalign="center" style="labelBase" color=".7 .7 .7 1" font="subdyn_13" content="---" noclick="1" />
					<label name="craftingNewItemComponent{id}Value" fity="1" textalign="center" style="labelBase" color="1 1 1 1" font="maindyn_30" content="-" noclick="1" />
				</panel>
			</button>
			<panel name="craftingNewItemComponent{id}_gold_coverup" noclick="1" visible="0">
				<frame texture="/ui/shared/frames/std_btn_inset.tga" borderthickness="5s" bordercolor="0 0 0 .92" color="0 0 0 .92" />
				<panel noclick="1" grow="1" regrow="1" float="bottom" align="center" valign="center">
					<panel grow="1" regrow="1" height="26s" float="right" padding="4s">
						<label name="craftingNewItemComponent{id}_gold_cost" fitx="true" outline="1" outlinecolor="black" textvalign="center" style="labelBase" font="maindyn_26" content="+--" noclick="1"/>
						<image height="28s" width="28s" texture="/ui/elements:gold_coins" noclick="1" />
					</panel>
				</panel>
			</panel>
			<instance name="iconButton" id="craftingNewItemComponent{id}RemoveButton"
				icon="/ui/main/shared/textures/close.tga"
				width="26s" height="26s" x="5s" y="-7s" align="right" valign="top" borderthickness="4s"
			/>
			<instance name="DropLand" id="craftingNewItemComponent{id}DropTarget" />
			<instance name="DropLand" id="craftingNewItemComponent{id}ClickDropTarget" />
			<panel height="36s" width="36s" x="-8s" y="7s" align="left" valign="bottom" visible="1" noclick="true" passivechildren="true">
				<image name="craftingNewItemComponent{id}JublieImage" height="100%" width="100@" group="mainCraftingNewItemComponent_jublies" texture="/ui/main/crafting/textures/jublie.tga" color=".5 .5 .5 1" noclick="1" />
				<label name="craftingNewItemComponent{id}Cost" y="-1s" textalign="center" textvalign="center" style="labelBase" font="maindyn_20" color="1 1 1 1" content="" />
			</panel>
		</panel>
	</template>

