	<template name="raf_objective">
		<panel width="244s" height="100%">			
			<!-- Show When In Progress -->
			<panel visible="0" noclick="1"
				onloadlua="
					self:RegisterWatchLua('AccountProgression', function(widget, trigger)
						if (mainUI) and (mainUI.progression) and (mainUI.progression.stats) and (mainUI.progression.stats.referafriend) and (mainUI.progression.stats.referafriend.referrals) then
							if (mainUI.progression.stats.referafriend.referrals >= tonumber({requirement=-1})) and (tonumber({id=-1}) > mainUI.progression.stats.referafriend.referrals) then
								widget:SetVisible(1)
							else
								widget:SetVisible(0)
							end
						end
					end)
				"
			>								
				<frame texture="/ui/shared/frames/togglebox.tga" borderthickness="10s" width="-16s" align="center" height="32s" y="-26s" color="#52edff" />
				<frame texture="/ui/shared/frames/togglebox.tga" borderthickness="10s" width="-16s" align="center" height="32s" y="-26s" color="#1ae7ff" rendermode="additive" />				
				<frame texture="/ui/elements:rounded_bg_white" borderthickness="8s" color=".1 .1 .1 1"/>								
				<panel grow="1" regrow="1" y="-25s" height="22s" align="center" float="right" padding="5s" noclick="1">
					<label fitx="1" style="labelBase" font="maindyn_22" color="1 1 1 1" textalign="center" content="player_profile_referafriend_current" noclick="1" />
					<label fitx="1" style="h1" font="maindyn_22" textalign="center" content="---" noclick="1"
					onloadlua="
						self:RegisterWatchLua('AccountProgression', function(widget, trigger)
							if (mainUI) and (mainUI.progression) and (mainUI.progression.stats) and (mainUI.progression.stats.referafriend) and (mainUI.progression.stats.referafriend.referrals) then
								widget:SetText(mainUI.progression.stats.referafriend.referrals)
							end
						end)
					"
					/>
				</panel>
			</panel>
			
			<frame texture="/ui/shared/frames/rounded_bg_solid.tga" style="doubleFrameBg" borderthickness="8s"/>
			<frame texture="/ui/shared/frames/border_double.tga" style="doubleFrame" borderthickness="8s" color="invisible"/>	
			<panel grow="1" align="center" y="6s" float="right" padding="2s" noclick="1">
				<label fitx="1" style="labelBase" font="maindyn_24" content="profile_referafriend_send_{id}" noclick="1"/>
				<label fitx="1" style="labelBase" font="subdyn_11" content="AST" noclick="1"/>
			</panel>
			
			<instance name="hr" y="36s" align="center" width="-30s" />
			<instance name="raf_objective_{id}" />
			
			<!-- Show When Complete -->
			<frame visible="0" texture="/ui/elements:rounded_bg_white" borderthickness="8s" color="0 0 0 .6"
				onloadlua="
					self:RegisterWatchLua('AccountProgression', function(widget, trigger)
						if (mainUI) and (mainUI.progression) and (mainUI.progression.stats) and (mainUI.progression.stats.referafriend) and (mainUI.progression.stats.referafriend.referrals) then
							if (mainUI.progression.stats.referafriend.referrals >= tonumber({id=-1})) then
								widget:SetVisible(1)
							else
								widget:SetVisible(0)
							end
						end
					end)
				"
			>
				<frame texture="/ui/elements:thick" borderthickness="8s" color=".2 .35 .2 1"/>
				<image texture="/ui/main/shared/textures/stylized_check.tga" width="64s" height="64s" x="-14s" y="-17s" noclick="1" />				
			</frame>
			
		</panel>
	</template>

