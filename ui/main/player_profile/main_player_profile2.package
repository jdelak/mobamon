	<template name='profile_achievement_icon'>
		<panel name="profile_achievement_icon_{achievement}_container" noclick="1" width='{width=100@}' height='{height=100%}'>			
			<panel name="profile_achievement_icon_{achievement}_owned" visible="0"><!-- Owned Indication -->
				<frame texture="/ui/elements:squareglow" color=".4 .9 1 .5" borderthickness="8s" width="+6s" height="+6s" align="center" valign="center" rendermode="additive" />
				<modelpanel height="+20s" width="+20s" align="center" valign="center" visible="1"
					noclick="true"
					model="/shared/models/invis.mdf"
					effect="/ui/main/crafting/effects/alert.effect"
					onshowlua="self:SetEffect('/ui/main/crafting/effects/alert.effect')"
					onhidelua="self:SetEffect('')"
					camerapos="0 0 0"
					cameraangles="0 0 0"
					camerafov="40"
					depthclear="true"
					depthcompress="true"
				/>
			</panel>			
			<frame name="profile_achievement_icon_{achievement}_hoverBehind" texture="/ui/elements:squareglow" color="#971500" bordercolor="#971500" borderthickness="16s" width="+14s" height="+14s" align="center" valign="center" rendermode="additive" visible="0"/><!-- Back Hover -->						
			<!-- The onmouseoutlua is overridden by player_profile.lua for icons we own, so the lock doesn't fade back in.-->
			<panel name='profile_achievement_icon_{achievement}' texture="/ui/shared/textures/account_icons/{achievement}.tga"
				onmouseoverlua="
					simpleTipGrowYUpdate(true, nil, Translate('account_icon_{achievement}'), Translate('account_icon_desc_{achievement}'), 250)
					self:GetWidget('profile_achievement_icon_{achievement}_hover'):FadeIn(200)
					self:GetWidget('profile_achievement_icon_{achievement}_hoverBehind'):FadeIn(200)
					self:GetWidget('profile_achievement_icon_{achievement}_lock'):FadeOut(200)
					local container = self:GetWidget('profile_achievement_icon_{achievement}_container')
					ScaleInPlace(container, '160@', '160%', 150)
					container:BringToFront()
					container:GetParent():BringToFront()
				"
				onmouseoutlua="
					simpleTipGrowYUpdate(false)
					self:GetWidget('profile_achievement_icon_{achievement}_hover'):FadeOut(200)
					self:GetWidget('profile_achievement_icon_{achievement}_hoverBehind'):FadeOut(200)
					self:GetWidget('profile_achievement_icon_{achievement}_lock'):FadeIn(200)
					ScaleInPlace(self:GetWidget('profile_achievement_icon_{achievement}_container'), '{width=100@}', '{height=100%}', 150, false, true)
				"
				noclick="0"
			/>			
			<frame name='profile_achievement_icon_{achievement}_lock' noclick="1" texture="/ui/elements:rounded_bg_white" borderthickness="6s" color="0 0 0 0">
				<image texture="/ui/shared/textures/account_icons/{achievement}.tga" noclick="1" rendermode="grayscale" color=".26 .26 .26 .84" />
				<panel texture="/ui/main/shared/textures/lock2.tga" x="15%" y="-8%" width="52@" height="52%" align="right" valign="top" color=".8 .8 .8 1" noclick="1" visible="1" />
			</frame>			
			<frame name="profile_achievement_icon_{achievement}_hover" texture="/ui/shared/frames/border_double.tga" width="+2s" height="+2s" align="center" valign="center" bordercolor="#971500" borderthickness="18s" noclick="1" color="invisible" visible="0"/><!-- Hover Frame -->
		</panel>
	</template>
	
