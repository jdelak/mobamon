<?xml version="1.0" encoding="UTF-8"?>
<package>

	<style name="playerProfileAddItemListbox" style="simpleListbox" scrollbarplaceholder="false" scrollbaroffset="0.25h" wrap="row" itemwidth="15.5h" itemheight="15.5h" select="false" highlightcolor="invisible" highlightbordercolor="invisible" valign="bottom" height="-4.5h" exteriorscrollbars="false"/>
	<style name="playerProfileEloStarOrder1" color="1 1 1 1" width="160s" height="160s" align="center" valign="center" noclick="1" />
	<style name="playerProfileEloStarOrder2" color=".5 .5 .5 1" width="120s" height="120s" align="center" valign="center" noclick="1" />
