	<!-- Hero Progression Portraits - Remove when not needed -->
	<template name="playerProfileHeroEntry">
		<button name="playerProfileHeroEntry{id}" color="invisible" passivechildren="true">
			<image name="playerProfileHeroEntry{id}Icon" width="58@" height="58%" align="center" y="24s" noclick="1"/>
			<image name="playerProfileHeroEntry{id}Frame" texture="/ui/main/shared/textures/iconframe_interactive_txt.tga" width="100@" height="100%" align="center" color="1 1 1 1" noclick="1" />
			<!-- HOVER state -->
			<panel name="playerProfileHeroEntry{id}Hover" visible="0" noclick="1">
				<panel width="58@" height="58%" align="center" y="24s" color="0 0 0 .8" noclick="1"/>
				<image texture="/ui/main/shared/textures/iconframe_interactive_txt_hover.tga" width="100@" height="100%" align="center" noclick="1" />
				<label name="playerProfileHeroEntry{id}WinCount" y="38s" textalign="center" style="labelBase" font="maindyn_18" content="stat_name_hero_wins_x" noclick="1" />
				<label name="playerProfileHeroEntry{id}WinsReq" y="64s" width="-40s" align="center" textalign="center" wrap="1" style="labelBase" font="maindyn_18" color=".8 .8 .8 1" content="stat_name_hero_wins_next_star_x" noclick="1" />
			</panel>
			<!-- Mastered Hero -->
			<panel name="playerProfileHeroEntry{id}Mastered" visible="0" noclick="1">
				<modelpanel name="" visible="1" height="100%" width="100%"
					noclick="true"
					y="15s" x="2s"
					model="/shared/models/invis.mdf"
					effect="/ui/main/shared/effects/mastery_flames.effect"
					camerapos="26 0 50"
					cameraangles="0 0 0"
					camerafov="80"
					depthclear="true"
					depthcompress="true"
				/>
			</panel>

			<!-- Stars -->
			<panel y="-14s" width="-28s" height="21s" align="center" valign="bottom" float="right" padding="0" noclick="1">
				<instance name="playerProfileHeroEntry_level_star" index="1" id="{id}" color="1 1 85 0.25" />
				<instance name="playerProfileHeroEntry_level_star" index="2" id="{id}" color="1 1 1 0.45" />
				<instance name="playerProfileHeroEntry_level_star" index="3" id="{id}" color="1 1 85 0.25" />
				<instance name="playerProfileHeroEntry_level_star" index="4" id="{id}" color="1 1 1 0.45" />
				<instance name="playerProfileHeroEntry_level_star" index="5" id="{id}" color="1 1 85 0.25" />
			</panel>
		</button>
		<!-- ELO/Rank -->
		<image
			name="playerProfileHeroEntry{id}RankIcon"
			texture="/ui/main/shared/textures/elo_rank_1.tga" width="52s" height="52s" x="-5s" y="-6s" visible="0"
			onmouseoverlua="simpleTipGrowYUpdate(true, nil, Translate('player_rank_hero_1'), Translate('player_rank_hero_tip'), libGeneral.HtoP(34))"
			onmouseoutlua="simpleTipGrowYUpdate(false)"
		/>
	</template>

	<template name="profile_content_tab_tabbuttonState">
		<widgetstate statename="{statename=up}" x="{x}" y="{y}">
			<frame texture="{texture=/ui/shared/frames/stnd_btn_up.tga}" borderthickness="8s" color="{color=.8 .8 .8 1}" rendermode="grayscale"/>
			<image group="{id}_image_group" width="{iconsize}@" height="{iconsize}%" texture="{icon}" y="{iconY}" x="{iconX}" align="center" />
			<label y="-24s" textvalign="bottom" textalign="center" wrap="1" style="labelBase" color="1 1 1 1" font="maindyn_22" content="{label2}" group="{id}Label2Group" noclick="1" visible="0"/>
			<label width="-10s" align="center" y="-12s" textvalign="bottom" textalign="center" wrap="1" lineheight="17s" style="labelBase" color="1 1 1 1" font="maindyn_22" content="{label}" noclick="1"/>
		</widgetstate>
	</template>
	<template name="profile_content_tab">
		<panel name="{id}Container" y="{y}" width="100%" height="82@" align="center" noclick="1" sticky="{sticky=true}" stickytoinvis="false" visible="{visible=true}" >
			<button name="{id}" passivechildren="true" onloadlua="{onloadlua}" group="{group}" interactive="{interactive=false}" enabled="{enabled=true}" color="invisible" onclicklua="{onclicklua}">
				<instance name="profile_content_tab_tabbuttonState" label="{label}" id="{id}" label2="{label2}" icon="{icon}" iconsize="{iconsize=74}" iconY="{iconY}" iconX="{iconX}" statename="up"/>
				<instance name="profile_content_tab_tabbuttonState" label="{label}" id="{id}" label2="{label2}" icon="{icon}" iconsize="{iconsize=74}" iconY="{iconY}" iconX="{iconX}" color="1 1 1 1" statename="over"/>
				<instance name="profile_content_tab_tabbuttonState" label="{label}" id="{id}" label2="{label2}" icon="{icon}" iconsize="{iconsize=74}" iconY="{iconY}" iconX="{iconX}" statename="down" x="1" y="1"/>

				<instance
					name="NumberValue"
					id="{id}_count_label_parent" countShown="0"
					labelId="{id}_count_label"
					x="-4s" y="-6s" height="22s" width="24s" valign="top" align="left"
					label="5"
				/>

				<widgetstate statename="disabled">
					<frame texture="/ui/shared/frames/std_btn_inset.tga" borderthickness="8s" color="1 1 1 1" />
					<image x="31s" width="32s" height="64" align="right" valign="center" texture="/ui/main/shared/textures/submenu_arrow.tga" noclick="1" visible="{hasSubMenu=0}" />
					<image width="{iconsize=74}@" height="{iconsize=74}%" texture="{icon}" y="{iconY}" x="{iconX}" align="center" />
					<label y="-24s" textvalign="bottom" textalign="center" wrap="1" style="labelBase" color="1 1 1 1" font="maindyn_22" content="{label2}" group="{id}Label2Group" noclick="1" visible="0"/>
					<label width="-10s" align="center" y="-12s" textvalign="bottom" textalign="center" wrap="1" lineheight="17" style="labelBase" color="1 1 1 1" font="maindyn_22" content="{label}" noclick="1"/>
				</widgetstate>
			</button>
			<!-- Turn Off -->
			<frame name="{id}_turnoff" texture="/ui/shared/frames/stnd_btn_up.tga" borderthickness="8s" color=".2 .2 .2 .75" rendermode="grayscale" blockinput="1" visible="{turnOff=false}" />
		</panel>

	</template>

	<template name="profile_content_tabShort_tabbuttonState">
