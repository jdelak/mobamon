	<template name="playerProfileColumnEmpty">
		<panel name="playerProfileColumnEmpty{id}" height="15.5h" noclick="true">
			<frame texture="/ui/elements:rounded_bg_white" borderthickness="1.0h" color="#d7c9af" noclick="true" />
			<frame texture="/ui/elements:roundframe" color="0 0 0 1" borderthickness="1.0h" blockinput="false" noclick="true"/>
			<panel hmargin="0.8h" noclick="true" passivechildren="true">
				<label y="0.4h" textalign="center" font="maindyn_20" color="#3e371a" outline="true" outlinecolor="0 0 0 .15" content="player_profile_pins" />
				<label y="-0.5h" valign="bottom" wrap="true" textvalign="bottom" textalign="center" font="maindyn_14" color="#3e371a" content="player_profile_pin_body" />
			</panel>
			<image y="-2.7h" width="8.6h" height="8.6h" align="center" texture="/ui/_textures/icons/pin.tga" noclick="true" />
			<button name="playerProfileColumnEmpty{id}DropTargetCenter" color="#FFDD3366" visible="false">
				<panel name="playerProfileColumnEmpty{id}DropTargetCenterOver" color="0.15 0.15 0.15" rendermode="additive" visible="false"/>
			</button>
		</panel>
	</template>

	<template name="playerProfileRemoveOverlay">
		<panel name="{id}RemoveOverlay" noclick="true" passivechildren="true" visible="false">
			<frame texture="/ui/elements:rounded_bg_white" borderthickness="{hovercorner=1.0h}" color="0 0 0 .9" noclick="true"/>
			<panel hmargin="0.8h" vmargin="0.8h">
				<image width="3.2h" height="3.2h" valign="bottom" texture="/ui/_textures/icons/trash.tga" />
				<label width="65%" align="right" wrap="true" textvalign="bottom" textalign="right" font="maindyn_14" color="1 1 1 1" outline="true" outlinecolor="0 0 0 .7" content="Remove From Profile" />
			</panel>
		</panel>
	</template>

	<template name="replay_participant">
		<panel height="24s" color="invisible" watch="ReplayInfoPlayer{slot}" ontrigger="SetVisible(!StringEmpty(param0));">
			<panel height="2.2h" width="2.2h" valign="center">
				<image watch="ReplayInfoPlayer{slot}" ontrigger="SetTexture(param4);" />
				<frame color="invisible" bordercolor=".3 .3 .3 1" borderthickness="1" noclick="1" />
			</panel>
			<label x="3.2h" height="100%" textvalign="center" font="dyn_10" color="white" shadow="1" watch="ReplayInfoPlayer{slot}" ontrigger="SetText(param0); SetColor(param1);" />
		</panel>
	</template>

	<template name="playerProfileAchievement">
		<panel width="100%" height="100@">
			<panel width="80%" height="80%" align="center" valign="center" color=".09 .09 .09 1" noclick="1" />
			<image texture="/ui/main/shared/textures/iconframe_displayinfo.tga" noclick="1" />
			<image texture="/ui/main/player_profile/textures/achievement.tga" width="54@" height="54%" align="center" valign="center" rendermode="grayscale" color=".4 .4 .4 .2" />
		</panel>
	</template>

	<template name="playerProfileIcon">
		<panel name="playerProfileIcon_{index}" width="100@" height="100%"
			onmouseoverlua="
				self:GetWidget('playerProfileIcon_{index}_hover'):SetVisible(1)
			"
			onmouseoutlua="
				self:GetWidget('playerProfileIcon_{index}_hover'):SetVisible(0)
			"
			visible="0"
		>
			<image name="playerProfileIcon_{index}_icon" texture="/ui/shared/textures/account_icons/default.tga" noclick="1" />

			<!-- Hover State -->
			<frame name="playerProfileIcon_{index}_hover" texture="/ui/shared/frames/border_double.tga" bordercolor="#a33024" borderthickness="8s" noclick="1" color="invisible" visible="{playerProfileIconHover=false}"/>

			<!-- Locked - Grayscale and lock icon added  - Tooltip to explain how to earn? -->
			<panel name="playerProfileIcon_{index}_locked" visible="{playerProfileIconLocked=false}" noclick="1">
				<image texture="/ui/main/shared/textures/lock2.tga" x="8s" y="-10s" align="right" valign="top" width="45s" height="45s"  noclick="1"/>
			</panel>

		</panel>
	</template>

	<template name="playerProfileAccountRankStar">
		<panel width="100@" height="100%" noclick="1" >
			<image name="playerProfileAccountRankStar{id}Full" texture="/ui/shared/textures/star.tga" noclick="1"  />
			<panel name="playerProfileAccountRankStar{id}Partial" align="right" clip="1" noclick="1" color="invisible"  >
				<image texture="/ui/shared/textures/star.tga" align="right" width="100@" rendermode="grayscale" color=".3 .3 .3 .9" noclick="1"  />
			</panel>
		</panel>
	</template>

