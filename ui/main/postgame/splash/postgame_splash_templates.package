<?xml version="1.0" encoding="UTF-8"?>
<package>

	<template name="postgame_summary_insert_upcoming_unlock_template">
		<panel name="postgame_summary_insert_account_progression_prize_parent_{index}" height="{height}" width="{width}" noclick="0" visible="0" sticky="1" stickytoinvis="0" >
