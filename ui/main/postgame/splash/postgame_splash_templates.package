			<image name="postgame_summary_insert_account_progression_prize_icon_{index}" texture="$checker" align="center" valign="center" noclick="1"/>
			<image name="postgame_summary_insert_account_progression_prize_unlocked_{index}" texture="/ui/main/shared/textures/herorole_frame.tga" align="center" valign="center" noclick="1"/>
			<image name="postgame_summary_insert_account_progression_prize_icon2_{index}" texture="$checker" align="center" valign="center" rendermode="grayscale" color="1 1 1 0.8" noclick="1"/>
					
			<image name="postgame_summary_insert_account_progression_prize_lock_{index}" texture="/ui/main/shared/textures/herorole_frame_locked.tga" align="center" valign="center" noclick="1"/>
			
			<panel name="postgame_summary_insert_account_progression_prize_count_parent_{index}" noclick="1" visible="0">
				<image name="postgame_summary_insert_account_progression_prize_count_glow_{index}" texture="/ui/main/shared/textures/rectangle_shadow.tga" y="31s" valign="center" align="center" width="60s" height="24s" noclick="1" visible="1"  />			
				<image visible="1" texture="/ui/main/postgame/khanquest/textures/flare_seperator.tga" y="27s" width="250s" height="210s" align="center" valign="center" noclick="1" rendermode="additive" />			
				<label name="postgame_summary_insert_account_progression_prize_count_{index}" fity="1" y="-5s" textalign="center" textvalign="center" style="labelBase" color="#f2cc83" outlinecolor="#371d06" valign="bottom" font="maindyn_30" content=""/>
			</panel>
			
			<panel name="postgame_summary_insert_account_progression_unlock_effect_{index}" align="center" valign="center" visible="0" width="60%" height="60%" noclick="1" clip="1">
				<frame texture="/ui/shared/frames/rounded_bg_solid" color="invisible" borderthickness="6s" bordercolor="0.13 0.76 1 1" noclick="1"/>
				<panel color="0.13 0.76 1 0.9" noclick="1"/>
			</panel>
			
			<panel width="100%" height="100%" align="center" valign="center" visible="1" noclick="1" clip="1">
