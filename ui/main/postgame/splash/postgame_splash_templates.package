	<template name="postgame_award">
		<panel name="postgame_award{id}" width="90@" height="100%" passivechildren="true">
			<image name="postgame_award{id}Icon" texture="/ui/shared/textures/pack3.tga" width="80%" height="80@" align="center" y="10@"/>
			<label name="postgame_award{id}Label" textalign="center" textvalign="center" height="5.5h" color="white" outline="true" valign="bottom" font="maindyn_26" content="-"/>
		</panel>
	</template>
	
	<template name="postgame_awards_left">
		<panel x="25s" name="postgame_awards_left" width="360s" height="530s" y="25h" noclick="true" visible="false" >
			<scrollbox name="quest_endgame_list_entry_target" y="0h"
				visible="1" valign="top" align="left" clip="1" color="invisible"
				noclick="1" float="bottom" padding="1.5h"
				vscrollboxstep="4.0h"
				scrollbarplaceholder="0"
				scrolltexture="/ui/shared/textures/slider.tga"
				handleheight="64s"
				scrollbarsize="32s"
				exteriorscrollbars="1"
				vscrollbar="1"
				hscrollbar="0"
				horizontal="false"
				clipx="0"
				clipy="0"
				clipwidth="0"
				clipheight="0"
				scrollbaroffset="0"
				clipoffsetx="0"
				clipoffsety="0"
				onload="SetClipAreaToChild();"
				onshow="SetClipAreaToChild();"
				onslide=""
			>			

			</scrollbox>			
		</panel>
	</template>
	
	<template name="postgame_quest_accountlevel_container_prize_template">
		<panel height="100%" width="81@" noclick="1" sticky="1" stickytoinvis="0" visible="{visible=0}">
			<panel color="#090b0b" x="-4%" y="-5%" align="center" valign="center" width="82%" height="82@" noclick="1" />
			<image  x="-2%" y="-7%" align="center" valign="center" width="78%" height="78@" texture="{prizeIcon=$checker}" noclick="1" />			
			<image texture="/ui/main/shared/textures/iconframe_interactive_txt.tga" x="-2@" width="100@" align="center" noclick="1" />			
			<label x="-2%" y="-8%" textalign="center" textvalign="bottom" style="labelBase" font="subdyn_13" content="{prizeLabel=Missing!}" noclick="1" />
		</panel>
	</template>	
	
	<template name="postgame_quest_accountlevel_template" >
		<panel group="{group=postgame_quest_accountlevels}" width="800s" height="116s" noclick="0" sticky="0" stickytoinvis="0" >
			<image texture="/ui/main/quests/textures/quest_lifetime_base.tga" width="800@" height="100%" noclick="1" />
			<label x="24@" y="16%" style="h1" font="maindyn_26" content="{questName1}" noclick="1" />
			<label x="24@" y="40%" style="labelBase" color=".6 .6 .6 1" font="subdyn_15" content="{rewardText1Long}" noclick="1" />
			<panel x="0" height="50%" width="{progressBarWidth=88.1}%" valign="bottom" noclick="1">
				<image width="-200@" height="100%" align="center" texture="/ui/main/quests/textures/quest_lifetime_progressback_center.tga" uscale="256s" utile="true" onshow="SetUScale(GetHeight() # 's');" noclick="1" />
				<image width="200@" height="100%" align="left" texture="/ui/main/quests/textures/quest_lifetime_progressback_lcap.tga" noclick="1" />
				<image width="200@" height="100%" align="right" texture="/ui/main/quests/textures/quest_lifetime_progressback_rcap.tga" noclick="1" />
				<panel group="{group=postgame_quest_accountlevels}_progress" y="34.0%" width="-40s" height="45%" align="center" noclick="1">
					<instance name="progress_bar" id="postgame_quest_accountlevel_template_{id}_0" visible="{boosted_xp=0}" startwidth="{oldProgressPercent}%" width="{progressPercentBoosted=0}%" borderthickness="4s" barstyle="barCommon" color="cyan" />
					<instance name="progress_bar" id="postgame_quest_accountlevel_template_{id}_1" startwidth="{oldProgressPercent}%" width="{progressPercent}%" borderthickness="4s" barstyle="barCommon" color="#3fd149" />
					<instance name="progress_bar" id="postgame_quest_accountlevel_template_{id}_2" width="{oldProgressPercent}%" borderthickness="4s" barstyle="barDefault" color="#c1342a" />					
					<modelpanel model="/ui/_models/16to1/button.mdf" anim="idle" effect="/ui/main/shared/effects/questbar_completed.effect" 
						camerapos="0 -45 0" cameraangles="0 0 180" camerafov="70" cameranear="1" camerafar="1000" depthclear="false" depthcompress="true" noclick="true" 
						visible="{isComplete=1}" 
					/>
					<label y="-2s" fitx="1" align="center" textalign="center" textvalign="center" style="labelBase" font="maindyn_24" content="{progressLabel1}" noclick="1" />
				</panel>
			</panel>
			<panel x="-14@" y="-3%" grow="1" regrow="1" growinvis="false" height="100%" align="right" float="right" padding="0" noclick="1">
				<instance name="postgame_quest_accountlevel_container_prize_template" visible="{hasReward1=false}" prizeLabel="{rewardText1}" prizeIcon="{rewardIcon1}" />
				<instance name="postgame_quest_accountlevel_container_prize_template" visible="{hasReward2=false}" prizeLabel="{rewardText2}" prizeIcon="{rewardIcon2}" />
				<instance name="postgame_quest_accountlevel_container_prize_template" visible="{hasReward3=false}" prizeLabel="{rewardText3}" prizeIcon="{rewardIcon3}" />
			</panel>
		</panel>		
	</template>	
	
	<template name="postgame_summary_game_award_template">
		<panel name="postgame_summary_game_award_{index}" group="{group=postgame_quest_prize_template}" bitmaskfile="{prizeIcon=$checker}" usebitmask="1" align="{align=left}" valign="top" width="242s" height="280s" passivechildren="true" visible="{visible=0}" noclick="0" sticky="0" stickytoinvis="0" 
			onmouseoverlua="
				if (not Empty([==[{title}]==])) then 
					simpleTipGrowYUpdate(true, nil, [==[{title}]==], [==[{desc}]==], libGeneral.HtoP(32)) 
				end"
			onmouseoutlua="simpleTipGrowYUpdate(false)"
		>		
			<image name="postgame_summary_game_award_{index}Icon" texture="{prizeIcon=$checker}" width="98%" height="98@" align="center" y="2@" noclick="1"/>			
			<label name="postgame_summary_game_award_{index}Label" textalign="center" textvalign="center" height="5.5h" color="white" outline="true" wrap="1" valign="bottom" font="maindyn_26" content="{prizeLabel=Missing!}" noclick="1"/>
		</panel>
	</template>	
	
