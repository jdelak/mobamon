				<image y="-110s" x="-110s" name="postgame_summary_insert_account_progression_unlock_shimmer3_{index}" texture="/ui/main/shared/textures/shimmer.tga" align="center" valign="center" color="0.13 0.76 1 0.4" noclick="1"/>
			</panel>
			
			<image name="postgame_summary_insert_account_progression_prize_shine_{index}" y="-48s" x="-48s" texture="/ui/main/shared/textures/shine.tga" align="center" valign="center" color="1 1 1 0.5" noclick="1"/>
			<image name="postgame_summary_insert_account_progression_prize_shine2_{index}" y="46s" x="46s" texture="/ui/main/shared/textures/shine.tga" align="center" valign="center" color="1 1 1 0.4" noclick="1"/>
