<?xml version="1.0" encoding="UTF-8"?>
<package>

	<template name="watch_content_tabbuttonState">
		<widgetstate statename="{statename=up}" x="{x}" y="{y}">
			<frame texture="{texture=/ui/shared/frames/stnd_btn_up.tga}" borderthickness="8s" color="{color=.8 .8 .8 1}" rendermode="grayscale"/>
			<image group="{id}_image_group" width="{iconsize}@" height="{iconsize}%" texture="{icon}" y="{iconY}" x="{iconX}" align="center" />
			<label y="-22s" textvalign="bottom" textalign="center" style="labelBase" color="1 1 1 1" font="subdyn_15" content="{label2}" noclick="1"/>
			<label y="-6s" textvalign="bottom" textalign="center" style="labelBase" color="1 1 1 1" font="subdyn_15" content="{label}" noclick="1"/>
		</widgetstate>
	</template>
	<template name="watch_content_tab">		
		<panel name="{id}Container" y="{y}" width="100%" height="82@" align="center" sticky="{sticky=true}" stickytoinvis="false" visible="{visible=true}" noclick="1">
			<button name="{id}Btn" passivechildren="true" onloadlua="{onloadlua}" onmouseoverlua="{onmouseoverlua}" onmouseoutlua="{onmouseoutlua}" group="{group}" visible="{visible=true}" interactive="{interactive=false}" enabled="{enabled=true}" color="invisible" onclicklua="{onclicklua}">
				<instance name="watch_content_tabbuttonState" id="{id}" label="{label}" label2="{label2}" icon="{icon}" iconsize="{iconsize=74}" iconY="{iconY}" iconX="{iconX}" statename="up"/>
				<instance name="watch_content_tabbuttonState" id="{id}" label="{label}" label2="{label2}" icon="{icon}" iconsize="{iconsize=74}" iconY="{iconY}" iconX="{iconX}" color="1 1 1 1" statename="over"/>
				<instance name="watch_content_tabbuttonState" id="{id}" label="{label}" label2="{label2}" icon="{icon}" iconsize="{iconsize=74}" iconY="{iconY}" iconX="{iconX}" statename="down" x="1" y="1"/>
				
				<instance
					name="NumberValue"
					id="{id}_count_label_parent" countShown="0"
					labelId="{id}_count_label"
					x="-4s" y="-6s" height="22s" width="24s" valign="top" align="left"
					label=""
				/>					
				
				<widgetstate statename="disabled">
					<frame texture="/ui/shared/frames/std_btn_inset.tga" borderthickness="8s" color="1 1 1 1" />
					<image width="{iconsize=74}@" height="{iconsize=76}%" texture="{icon}" y="{iconY}" x="{iconX}" align="center" />
					<label y="-22s" textvalign="bottom" textalign="center" style="labelBase" color="1 1 1 1" font="subdyn_15" content="{label2}" noclick="1"/>
					<label y="-6s" textvalign="bottom" textalign="center" style="labelBase" color="1 1 1 1" font="subdyn_15" content="{label}" noclick="1"/>
				</widgetstate>
			</button>
		</panel>
	</template>	

