	<template name="watch_recent_matches_template">
		<panel noclick="0"
			onmouseoverlua="self:GetWidget('watch_recent_matches_{index}_selected'):FadeIn(125)"
			onmouseoutlua="self:GetWidget('watch_recent_matches_{index}_selected'):FadeOut(125)"
		>	
			<frame texture="/ui/shared/frames/rounded_bg_solid.tga" borderthickness="8s" color=".1 .1 .1 0.7" noclick="1"/>
			<frame texture="/ui/shared/frames/border_double.tga" style="doubleFrame" borderthickness="8s" color="invisible" noclick="1"/>
			
			<panel width="100%" height="100%" float="right" padding="4s" noclick="1">
				<image width="2.5%" height="70%" texture="/ui/main/watch/textures/watch_replay_dlconfirmed.tga" valign="center" noclick="1"
					oninstantiatelua=" 
						if (not {hasReplay=false}) then 
							self:SetRenderMode('grayscale') 
							self:SetColor('.6 .6 .6 1') 
						end
					"				
				/> 
				<panel height="70%" width="70@" color=".23 .23 .23 1" noclick="1">
					<image width="-4s" height="-4s" valign="center" align="center" texture="{heroIcon=$checker}" noclick="1" 
						oninstantiatelua=" 
							self:RegisterWatchLua('WatchStateTrigger', function(widget, trigger)
								if (trigger.matchIndex == '{index}') and (not Empty(trigger.currentHeroIcon)) then
									widget:SetTexture(trigger.currentHeroIcon)
								end
							end, false, nil, 'matchIndex', 'currentHeroIcon')
						"						
					/>
				</panel>
				<label width="37%" style="labelBase" color="{fontColor=1 1 1 1}" font="{font=maindyn_18}" textalign="left" textvalign="center" valign="center" content="{label1}" noclick="1"/>
				<label width="21%" style="labelBase" color="{fontColor=1 1 1 1}" font="{font=maindyn_18}" textalign="center" textvalign="center" valign="center" content="{label3}" noclick="1"/>
				<label width="21%" style="labelBase" color="{fontColor=1 1 1 1}" font="{font=maindyn_18}" textalign="center" textvalign="center" valign="center" content="{label2}" noclick="1"/>
				<panel width="69@" height="69%" align="right" texture="/ui/main/shared/textures/scroll.tga" valign="center" visible="1" noclick="0" 
					oninstantiatelua=" 
						if ('{matchNote}' ~= '') then 
							self:SetRenderMode('normal')
						else
							self:SetRenderMode('grayscale')
						end 
						if ('{match_id}' == '0') then 
							self:SetVisible(0)
						end
					"			
					onmouseoverlua="
						if ('{matchNote}' ~= '') then 
							simpleTipGrowYUpdate(true, '/ui/main/shared/textures/scroll.tga', '{matchName}', '{matchNote=}', self:GetWidthFromString('350s'))
						else
							simpleTipGrowYUpdate(true, '/ui/main/shared/textures/scroll.tga', '{matchName}', Translate('watch_match_info_notes_create'), self:GetWidthFromString('350s'))
						end
						self:GetWidget('watch_recent_matches_{index}_selected'):FadeIn(125)
					"
					onmouseoutlua="
						simpleTipGrowYUpdate(false)
						self:GetWidget('watch_recent_matches_{index}_selected'):FadeOut(125)
					"
					onclicklua="
						mainUI.watch.EditMatchInfo('{replayPath}', '{match_id}')
					"				
				/>
			</panel>
		</panel>
		<panel name="watch_recent_matches_{index}_current" visible="0" noclick="1"
			oninstantiatelua="
				self:RegisterWatchLua('WatchStateTrigger', function(widget, trigger)
					widget:SetVisible(trigger.matchIndex == '{index}')
				end, false, nil, 'matchIndex')
				self:SetVisible(LuaTrigger.GetTrigger('WatchStateTrigger').matchIndex == '{index}')
			"		
		>
			<frame texture="/ui/shared/frames/framehighlight.tga" bordercolor="1 1 1 .15" color="1 1 1 .08" borderthickness="8s" noclick="1"/>
			<frame texture="/ui/shared/frames/border_double.tga" style="doubleFrame" bordercolor="0.07 0.89 0.88 0.5" color="1 1 1 0" borderthickness="8s" noclick="1"/>
		</panel>
		<panel name="watch_recent_matches_{index}_selected" visible="0" noclick="1">
			<frame texture="/ui/shared/frames/framehighlight.tga" bordercolor="1 1 1 .15" color="1 1 1 .15" borderthickness="8s" noclick="1"/>
			<frame texture="/ui/shared/frames/border_double.tga" style="doubleFrame" bordercolor="0.07 0.89 0.88 1" color="1 1 1 0" borderthickness="8s" noclick="1"/>
		</panel>
	</template>
	
	<style name="watch_loading_hero_slice" noclick="1" width="19%" height="100%" clip="1" />	
	<style name="watch_loading_hero_slice_image" noclick="1" />	

	<template name="watch_loading_team_item_template" >
		<panel name="watch_loading_team_{playerid}" style="watch_loading_hero_slice" >
			<splashimage
				name="watch_loading_team_{playerid}_image"
				noclick="true"
				visible="false"
				bgcolor="black"
				style="watch_loading_hero_slice_image"
				image=""
				hflip="{hflip}"
				view="clip"
				onloadlua="
					self:RegisterWatchLua('ReplayInfoPlayer{playerid}', function(widget, trigger)
						if trigger.heroName and (not Empty(trigger.heroName)) then
							local conceptArtPath = '/ui/game/loading/textures/'
							conceptArtPath = conceptArtPath .. string.lower(trigger.heroName) .. '_default.jpg'
							widget:SetImage(conceptArtPath)
							widget:SetVisible(true) 
							if (trigger.name == GetAccountName()) then
								if (tonumber({playerid}) >= 4) then
									GetWidget('watch_loading_team_1_myteam_indicator'):SetVisible(0)
									GetWidget('watch_loading_team_2_myteam_indicator'):SetVisible(1)
								else
									GetWidget('watch_loading_team_1_myteam_indicator'):SetVisible(1)
									GetWidget('watch_loading_team_2_myteam_indicator'):SetVisible(0)
								end
							end
						end
					end)
					self:RegisterWatchLua('WatchStateTrigger', function(widget, trigger)
						if (not Empty(trigger.heroIcon{playerid})) then
							local conceptArtPath = '/ui/game/loading/textures/'
							conceptArtPath = conceptArtPath .. string.lower(trigger.heroIcon{playerid}) .. '_default.jpg'						
							widget:SetImage(conceptArtPath)
							widget:SetVisible(true) 
						end
					end, false, nil, 'heroIcon{playerid}')
				"
				onhidelua=""				
			/>
		</panel>
	</template>

	<template name="mini_postgame_scoreboard_row">
		<panel name="{prefix=postgame}_scoreboard_row_{index}" width="390s" height="46s" align="center" float="right" padding="4s" noclick="1">
