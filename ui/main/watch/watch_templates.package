			</panel>
			<panel width="10.3%" height="10@" color=".07 .07 .07 1">
				<instance name="postgame_scoreboard_inventory" prefix="{prefix=postgame}" index="{index}" itemindex="4" icon="/items/recipes/damage/frostbrand/icon.tga" id="3"/>
			</panel>
			<panel width="10.3%" height="10@" color=".07 .07 .07 1">
				<instance name="postgame_scoreboard_inventory" prefix="{prefix=postgame}" index="{index}" itemindex="5" icon="/items/recipes/damage/gauntlet/icon.tga" id="4"/>
			</panel>
			<panel width="10.3%" height="10@" color=".07 .07 .07 1">
				<instance name="postgame_scoreboard_inventory" prefix="{prefix=postgame}" index="{index}" itemindex="6" icon="/items/recipes/damage/shieldbreaker/icon.tga" id="5"/>
			</panel>
