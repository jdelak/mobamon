			</panel>
			<panel width="10.3%" height="10@" color=".07 .07 .07 1">
				<instance name="postgame_scoreboard_inventory" prefix="{prefix=postgame}" index="{index}" itemindex="1" icon="/items/recipes/damage/bloodthirster/icon.tga" id="0"/>
			</panel>
			<panel width="10.3%" height="10@" color=".07 .07 .07 1">
				<instance name="postgame_scoreboard_inventory" prefix="{prefix=postgame}" index="{index}" itemindex="2" icon="/items/recipes/damage/codex/icon.tga" id="1"/>
			</panel>
			<panel width="10.3%" height="10@" color=".07 .07 .07 1">
