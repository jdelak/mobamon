	<template name="watchNavItem">
		<panel name="watch_nav_slot_{id}" width="100@" height="100%" color="black" visible="0">
			<frame borderthickness="1" color="#191919" bordercolor="#191919" noclick="true"/>
			<image name="watch_nav_slot_{id}Icon" texture="/ui/main/watch/textures/watch_empty_slot.tga" noclick="true"/>
