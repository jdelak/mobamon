		</widgetstate>
	</template>

	<template name="splashimage_template">
		<splashimage
			y="36s"
			noclick="1"
			visible="0"
			image="{image}"
			bgcolor="black"
			view="clip"
			onhidelua=""
		/>
	</template>

	<template name="selection_crafteditem_item_template" >
		<panel name="selection_crafteditem_item_{index}_parent" noclick="0" height="100%" width="100@" color=".1 .1 .1 1" >
