		</panel>
	</template>

	<template name="party_finder_region_item">
		<panel noclick="1" group="party_finder_region_items" >
			<label y="-2s" x="9s" style="labelBase" font="maindyn_18" textvalign="center" noclick="1" content="{label}" />
			<image y="-1s" width="80@" x="-120@" height="80%" align="right" valign="center" noclick="true" texture="{texture=$invis}"/>
		</panel>
	</template>

	<template name="team_selector_region_combobox">
