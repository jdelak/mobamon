				depthclear="true" 
				depthcompress="false"
				lookat="false"

				modelpos="0 -330 0"
				modelangles="8 0 0" 

				anim="idle"	
				rotation="90"	

				modelscale="0.75"	
			/>	
		</panel>		
		<panel name="generic_dialog_box_insert" noclick="1" align="center" valign="center" width="42h" height="6h" grow="1" regrow="1" growinvis="0" float="bottom" padding="0.75h" >
