<?xml version="1.0" encoding="UTF-8"?>
<package>

	<template name="main_login_prompt_eula_textbuffer_template" >
		<textbuffer 
			name="main_login_prompt_eula_textbuffer" wrap="1" noclick="0" editable="0" file="{file=:/tos.txt}"
			style="textBox1Input"  
			color="invisible"
			font="subdyn_15" textcolor="1 1 1 1"
			onesc=""
			onenter=""
			scrolltexture="/ui/shared/textures/slider.tga" scrollbarsize="40s"
			handleheight="80s" handlewidth="40s"
			exteriorscrollbars="1"
		/>		
	</template>
	
	<panel name="main_login_prompt_eula" noclick="0" visible="0" color="0 0 0 0.8">
		<panel noclick="1" align="center" valign="center" width="800s" height="500s" >
			<instance name="dialogWrapper2" noclick="1"/>
			<frame texture="/ui/shared/frames/text_input.tga" bordercolor=".2 .2 .2 1" color=".2 .2 .2 1" borderthickness="18s" noclick="1"/>
			
			<panel noclick="1" y="-90s" valign="top" align="left" float="bottom" >
				<label textalign="center" fity="1" style="h1" font="maindyn_40" content="tos_prompt_header" noclick="1" />
				<label textalign="center" fity="1" style="labelBase" wrap="1" font="maindyn_22" content="tos_prompt_desc" noclick="1" />			
			</panel>
			
			<panel name="main_login_prompt_eula_textbuffer_insert" height="-20s" y="7s" x="15s" valign="top" align="left" width="-30s" 
				onshowlua="
					self:Instantiate('main_login_prompt_eula_textbuffer_template', 'file', ':/tos_' .. GetCvarString('host_language') ..'.txt')
					self:ClearCallback('onshow')
				"
			>
			
			</panel>

			<panel noclick="1" height="80s" width="338s" y="95s" valign="bottom" align="right" >
