	<panel name="generic_dialog_box" width="100w" height="100h" color="invisible" visible="0"
		onevent2lua="ClickButton(self, 'generic_dialog_button_2')"
		onevent1lua="ClickButton(self, 'generic_dialog_button_1')"
	>
		<textbox visible="1" color="invisible" height="0" width="0" font="maindyn_14"
			maxlength="0"
			onesc="CallEventParamsX('generic_dialog_box', 2)"
			onenter="CallEventParamsX('generic_dialog_box', 1)"
			onshow="SetFocus(true); SetDefaultFocus(true)"			
		/>		
		<panel name="generic_dialog_box_bg" color="0 0 0 0.3" visible="0" />
		<panel name="generic_dialog_flair" noclick="1" align="center" visible="0"
			onshowlua="
				self:SetHeight(self:GetWidget('generic_dialog_box_insert'):GetHeight() * 3)
				self:SetWidth(self:GetWidget('generic_dialog_box_insert'):GetWidth() * 3)
				self:SetY(self:GetWidget('generic_dialog_box_insert'):GetY())
				self:SetY(self:GetY() + (50) )
				self:GetWidget('generic_dialog_flair_label_1'):SetText(Translate('options_cloud_flair_1'))
				self:GetWidget('generic_dialog_flair_label_1'):SetVisible(1)
				self:GetWidget('generic_dialog_flair_throb'):SetVisible(1)
				self:GetWidget('generic_dialog_flair_effect'):SetVisible(0)
				local widget = self
				libThread.threadFunc(function()	
					wait(750)
					widget:GetWidget('generic_dialog_flair_label_1'):SetText(Translate('options_cloud_flair_2'))
					widget:GetWidget('generic_dialog_flair_throb'):FadeOut(250)
					widget:GetWidget('generic_dialog_flair_effect'):SetVisible(1)
					wait(1000)
					widget:GetWidget('generic_dialog_flair_label_1'):FadeOut(750)
				end)
			"
		>
			<label name="generic_dialog_flair_label_1" y="33" fity="true" fitx="1" align="center" valign="center" textvalign="center" textalign="center" font="maindyn_22" color="white" outline="true" outlinecolor="0 0 0 .7" 
				content="options_cloud_flair_1" noclick="1" sticky="0" stickytoinvis="0"
