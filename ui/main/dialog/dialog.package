	<!-- no chat server blackout overlay - this must be behind the generic dialog -->
	<panel name="main_chat_no_connection_overlay" noclick="1" blockinput="0" visible="0" 
		onshowlua="
			if (Strife_Region.activeRegion) and (not Strife_Region.regionTable[Strife_Region.activeRegion].isProduction) then
				libThread.threadFunc(function()	
					wait(3000)				
					GetWidget('main_chat_no_connection_overlay_button'):FadeIn(500)
				end)
			end
		"
		onhidelua="
			self:GetWidget('main_chat_no_connection_overlay_button'):FadeOut(500)
		"
	>
		<panel height="-40s" valign="bottom" noclick="0" blockinput="1" /><!-- Blocks Input For screen below the header drag -->
		<panel width="200s" noclick="0" blockinput="1" /><!-- Blocks input to player card -->
		<panel name="main_chat_no_connection_overlay_dimmer" noclick="1" visible="0" color="0 0 0 0.35" />	
		<animatedimage name="main_chat_no_connection_overlay_wheel" texture="/ui/shared/throb/throb.tga" noclick="1" loop="1" fps="30" onshow="StartAnim(1);"
			y="-64s" height="128s" width="128s" align="center" valign="center" 
		/>		
		<label name="main_chat_no_connection_overlay_label" fitx="1" align="center" valign="center" fity="1" font="maindyn_36" content="" y="39s" color=".7 .7 .7 1" shadow="1" outline="1" />
		
		<instance id="main_chat_no_connection_overlay_button" height="64s" width="520s" y="-150s" labelX="0" enabled="1" visible="0" align="center" valign="bottom"
			name="standardButton"
			label="I Don't Need The Chat Server"
			font="maindyn_30"
			onclicklua="self:GetWidget('main_chat_no_connection_overlay'):FadeOut(500) Set('ui_dont_require_chat_server', 'true', 'bool')"
		/>			
	</panel>
	
