						</panel>
					</panel>
				</panel>
			</panel>
			
			<panel grow="1" regrow="1" growinvis="0" width="100%" align="center" valign="top" float="bottom" >
				<label name="generic_dialog_label_1" fity="true" fitypadding="2.0h" wrap="true" valign="top" textvalign="center" textalign="center" font="maindyn_22" style="labelBase" content="" noclick="1" sticky="0" stickytoinvis="0"/>
