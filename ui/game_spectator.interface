<?xml version="1.0" encoding="UTF-8"?>
<interface name="game_spectator" scenex="0" sceney="0" scenewidth="100%" sceneheight="100%">

	<!--<include file="/ui/shared_templates_main.package"/>-->
	<trigger name="gameToggleMenu"/>
	<trigger name="gameToggleScoreboard"/>
	<trigger name="SpecCinematicMode"/>
	<childinterface file="/ui/game/shared/game_styles.interface"/>
	<include file="/ui/shared/templates_elements_simple.package"/>	
	
	
	<style name="gameHotkeyLabel" color="black" outline="true" outlinecolor="#DDDDDD" font="maindyn_14" textalign="center" textvalign="center" noclick="true"/>
	
	<!-- Spec templates -->
	<template name="spec_stats_button">
		<panel name="{id}" x="{x}" y="{y}" width="{width=22.6%}" height="{height=100%}" valign="center" passivechildren="true" color="invisible" onclicklua="{onclicklua}">
			<frame texture="{texture=/ui/shared/frames/stnd_btn_up.tga}" borderthickness="0.8h" color="{color=.8 .8 .8 1}" rendermode="grayscale"/>
			<frame name="{id}current" texture="/ui/shared/frames/std_btn_inset.tga" borderthickness="0.8h" color="1 1 1 .8" visible="0"  />
			<panel y="-0.1h" grow="true" growinvis="0" height="100%" align="center" float="right" padding="0.5h" noclick="1">
				<panel width="{iconsize}@" height="100%" noclick="1">
					<image width="100%" height="100@" texture="{icon}" y="{iconY}" x="{iconX}" valign="center" />
				</panel>
				<label fitx="1" textvalign="center" style="labelBase" color="1 1 1 1" font="{font=maindyn_18}" content="{label}" noclick="1" visible="{hasLabel=1}"/>
			</panel>
		</panel>
	</template>
	
	<!-- Selected Hero Item -->
	<template name="specSelectedHeroItem">
		<panel x="{x}" y="{y}" width="3.4h" height="3.4h" noclick="true">
			<image texture="/ui/shared/textures/pack2.tga" noclick="true"/>
			<panel name="specSelectedUnitHeroItem{id=0}" passivechildren="true">
				<piegraph start="270" end="-90" square="true" color="0 0 0 0.7" noclick="true" name="specSelectedUnitHeroItem{id=0}CooldownPie"/>
				<label textalign="center" textvalign="center" font="maindyn_11" style="whiteTextColor" outline="true" name="specSelectedUnitHeroItem{id=0}Cooldown" />
				<image name="specSelectedUnitHeroItem{id=0}Icon"/>
			</panel>
		</panel>
	</template>

	<!-- Selected Hero Ability -->
	<template name="specSelectedHeroAbility">
		<panel x="{x}" width="100@" height="100%" name="specSelectedUnitHeroAbility{id=0}" passivechildren="true">			
			<image name="specSelectedUnitHeroAbility{id=0}Icon"/>
			<piegraph start="270" end="-90" square="true" color="0 0 0 0.7" noclick="true" name="specSelectedUnitHeroAbility{id=0}CooldownPie"/>
			<label textalign="center" textvalign="center" font="maindyn_12" style="whiteTextColor" outline="true" name="specSelectedUnitHeroAbility{id=0}Cooldown" />			
		</panel>
	</template>
	
	<!-- Selected Hero Stat -->
	<template name="specSelectedHeroStat">
		<panel width="100%" height="145@">
			<image x="1" y="1" texture="{icon}" width="100%" height="100@" color="0 0 0 .8" />
			<image texture="{icon}" width="100%" height="100@" color="{color}" />
			<label name="{id}" valign="bottom" fity="1" style="labelBase" outlinecolor="0 0 0 .6" font="subdyn_10" textalign="center" color="{color}" content="---" noclick="1"/>
		</panel>
	</template>

	<!-- =========== Basic (inherent) =========== -->
	
	<template name="gameInventoryBasicBackers">
		<panel name="gameInventory{id}{suffix}Container" group="gameInventory{id}{suffix}Containers" style="{containerStyle=gameInventoryItemContainer}">
			<frame texture="/ui/elements:rounded_bg_white" color="#333333" borderthickness="0.5h" noclick="true"/>
			<frame texture="/ui/elements:thick" color="black" borderthickness="0.5h" noclick="true"/>
			<panel style="gameInventoryItemIconSpace" texture="/ui/elements:rounded_bg_white_vert" color="#0B0B0B"/>
		</panel>
	</template>

	<template name="gameBindableHotkeyButton">
		<panel name="{id}" style="{style}" align="{align}" valign="{valign}" width="{width=100%}" height="{height=100%}" x="{x=0h}" noclick="true">
			<panel name="{id}Body" align="center" valign="center" noclick="true" passivechildren="true">
				<frame name="{id}Backer" texture="/ui/elements:key" visible="1" borderthickness="{borderthickness=33@}"/>
				<frame name="{id}Highlight" texture="/ui/elements:rounded_bg_white" rendermode="additive" color="#FFCC0011"/>
				<label name="{id}Label" style="gameHotkeyLabel" font="{font=maindyn_14}" content="-" y="{labelY}"
					onshowlua="{onrefreshlua}"
					onrefreshlua="{onrefreshlua}"
				/>
			</panel>
			<button name="{id}ButtonTip" hmargin="5@" vmargin="5%" noclick="false" interactive="false" color="invisible"/>
			<button name="{id}Button" hmargin="5@" vmargin="5%" noclick="true" interactive="false" color="invisible"
				onloadlua="libButton2.initializeButton(self, '{id}', 'key')"
			/>
		</panel>
	</template>
	
	<!-- ======================================== -->	
	
	<style name="gameMinimapContainers" align="right" valign="bottom" x="0.5h" y="0.5h" width="27h" height="27h" noclick="true"/>
	<style name="gameMinimapHeader" noclick="true" width="100%" height="25@" align="right" x="0" y="-4.1h"/>
	<style name="gameMinimapInset" x="-0.2h" y="-0.2h" hmargin="0.8h" vmargin="0.8h" noclick="true"/>
	<style name="gameMinimapGoldContainer" width="43%" height="11@" x="5%" y="4.2@" noclick="true" />
	
	<style name="gameMinimapIconLeft" width="9.85%" height="9.85@" align="center" x="8.35%" y="2.25@" noclick="true"/>
	<style name="gameMinimapIconCinema" width="13.2%" height="13.2@" y="3.2@" align="center" x="40.8%" noclick="true"/>
	<style name="gameMinimapIconRight" width="13.2%" height="13.2@" y="3.2@" align="center" x="28.6%" noclick="true"/>	
	
	<style name="gamePlayerInfoExpanded_style" y="104%" visible="0" noclick="1" passivechildren="1" />
	<style name="gameHeroInfoExpanded_style" y="104%" visible="0" noclick="1" passivechildren="1" />

	<style name="playerDisconnectIcon" texture="/ui/elements:disconnect" align="center" valign="top" width="60%" height="60@" y="-0.5h" noclick="true" visible="0"/>
	<style name="playerLoadIcon" texture="/ui/elements:icon_loading" align="center" valign="top" width="60%" height="60@" y="-0.5h" noclick="true" visible="0"/>
	<style name="playerLevelupIcon" texture="/shared/effects/stextures/plus.tga" align="center" color="1 1 1 1" valign="center" y="48%" width="38%" height="38@" noclick="true" visible="0"/>
	<style name="abilityLevelupIcon" texture="/shared/effects/stextures/plus.tga" align="center" color="1 1 1 1" valign="center" y="0%" width="98%" height="98@" noclick="true" visible="0"/>
	<style name="playerDeadIcon" texture="/ui/game/unit_frames/textures/dead.tga" noclick="true" width="52%" height="52@" x="0" y="10%" valign="bottom" visible="0"/>
	<style name="playerMVPIcon" texture="/ui/game/unit_frames/textures/mvp.tga" noclick="true" align="right" width="45%" height="45@" x="10%" y="-8%" visible="0"/>
	
	<style name="playerRespawnLabel" textalign="center" textvalign="center" noclick="true" />
	
	<style name="playerInfoBar" height="7.8h" width="7.8h" align="center" y="0.5h" noclick="true"/>
	<style name="gamePlayerInfoContainer" width="175@" height="100%" noclick="true"/>
	<style name="gamePlayerInfoIconSpace" y="5%" width="80@" vmargin="10%" x="16%" noclick="true"/>
	<style name="gameHeroInfoContainer" width="100@" height="100%" noclick="true"/>
	
	<style name="gameHeroLevelLabelSelf" color="white" outline="true" font="maindyn_16" x="0" y="-25%" noclick="true" textalign="center"/>
	<style name="gameHeroLevelLabel" color="white" outline="true" font="maindyn_16" x="0" y="-6%" noclick="true" textalign="center"/>

	<style name="gameHeroInfoIconInset" y="9%" hmargin="11@" vmargin="11%" noclick="true"/>	
	
	
	
	<template name="pinButton">
		<button name="{id}" color="invisible" width="{width=100%}" height="{height=100%}" x="{x}" y="{y}" align="{align}" valign="{valign}" visible="{visible=true}" sticky="{sticky}" stickytoinvis="{stickytoinvis}" interactive="{interactive=false}">
			<panel align="center" valign="center" name="{id}Body">
				<frame texture="/ui/elements:rounded_button_bg_white" borderthickness="0.35h" color="#444444"/>
				<image name="{id}PinFlair" align="center" valign="center" width="-25@" height="-25%" texture="/ui/elements:pulse" color="#00BFFF" visible="0"/>
				<image name="{id}Pin" align="center" valign="center" width="-25@" height="-25%" texture="/ui/elements:pin" color="#666666"/>
			</panel>
		</button>
	</template>	
	
	<!-- CUSTOM SPECTATOR HERO FRAMES -->
	
	<!-- TEMPLATES -->
	<template name="spec_hero_portrait" >
		<image name="{id}" width="6.8h" height="6.8h" valign="bottom" align="center" hflip="{hflip}" texture="/ui/game/shared/textures/krytos.png"/>
		<panel align="{align}" x="-0.9h" y="0.8h" valign="bottom" width="52@" height="52%" noclick="1">
			<image name="{id}_level_image" noclick="1" align="center" valign="center" texture="/ui/specui/textures/levelBacking.tga"/>
			<label name="{id}_level_label" noclick="1" y="-0.1h" textalign="center" textvalign="center" font="subdyn_9" style="labelBase" content="1" />
		</panel>
		<label noclick="1" name="{id}_RespawnTime" style="allyRespawnTimer"/>
		<label noclick="1" name="{id}_LoadPercent" style="allyLoadPercent"/>
		<label noclick="1" name="{id}_DisconnectTime" style="allyDisconnectTime"/>
		<label noclick="1" name="{id}_AFK" style="allyAFKIndicator" content="general_afk"/>
		
		<panel noclick="1" style="gameHeroInfoIconInset">
			<image noclick="1" name="{id}_DisconnectIcon" style="playerDisconnectIcon"/>
			<image noclick="1" name="{id}_LoadIcon" style="playerLoadIcon"/>
			<image noclick="1" name="{id}_DeadIcon" style="playerDeadIcon" visible="1"/>	
			<modelpanel noclick="1" name="{id}_Voip" style="allyVOIPIndicator"/>
		</panel>
		<image noclick="1" name="{id}_MVPIcon" style="playerMVPIcon" />
	</template>
	
	
	<template name="spec_hero_flyout" >
		<panel color="white" texture="{texture}"/>
	</template>
	
	<template name="spec_hero_ability" >
		<panel name="{id}" color="white" width="{width}" height="{height}" y="{y}">
			<!--
			<piegraph start="270" end="-90" square="true" color="0 0 0 0.7" noclick="true" name="{id}_pie"/>
			-->
			<label textalign="center" textvalign="center" font="subdyn_10" style="whiteTextColor" outline="true" name="{id}_cooldown" noclick="true"/>
			<frame texture="/ui/elements:rounded_corner_white" borderthickness="0.2h" color="0 0 0 1" noclick="true"/>
		</panel>
	</template>
	
	<template name="spec_hero_frame" >
		<panel name="{id}" height="11.8h" width="14.4h" sticky="1" float="{float}">		
			<image x="0" texture="/ui/specui/textures/spec_hero_frame_background.tga" width="200@" height="100%" hflip="{hflip}" align="{align}" />			
			
			<image name="{id}_portrait_side" x="0" texture="/ui/specui/textures/spec_hero_frame_heroset_left.tga" width="200@" height="100%" hflip="{hflip}" align="{align}" />			
			
			<panel x="0" sticky="1" width="-5.2h" align="right">
				
				<panel width="1.8h" x="{abilX}">
					<!-- abilities -->
					<panel name="{id}_ability_container" color="invisible" x="0h" y="1.7h" >
						<instance name="spec_hero_ability" y="0" width="100%" height="100@" id="{id}_abiltiy_1"/>
						<instance name="spec_hero_ability" y="2.2h" width="100%" height="100@" id="{id}_abiltiy_2"/>
						<instance name="spec_hero_ability" y="4.4h" width="100%" height="100@" id="{id}_abiltiy_3"/>
						<instance name="spec_hero_ability" y="6.8h" width="100%" height="100@" id="{id}_abiltiy_4"/>
					</panel>
				</panel>
				
				<panel width="5.4h" x="{portraitX}">
					<!-- hp -->
					<panel y="7.4h" color="#05460b" height="1.2h">
						<panel name="{id}_hp" texture="/ui/specui/textures/hp_bar.tga" width="6.78h" />
						<frame texture="/ui/elements:rounded_corner_white" borderthickness="0.3h" color=".15 .15 .15 1" noclick="true"/>
					</panel>			
					<!-- mana -->
					<panel y="8.85h" color="#002f3e" height="0.9h">
						<panel name="{id}_mana" texture="/ui/specui/textures/mana_bar.tga" width="6.78h" />
						<frame texture="/ui/elements:rounded_corner_white" borderthickness="0.3h" color=".13 .13 .3 1" noclick="true"/>
					</panel>
					<!-- portrait -->
					<panel color="invisible" y="1.2h" height="5.8h" width="5.8h" align="center" visible="1">
						<instance name="spec_hero_portrait" hflip="{hflip}" id="{id}_portrait" align="{align}"/>
					</panel>
				</panel>				
			</panel>
			<panel width="5.2h" sticky="1" align="left">
				<panel y="1.8h" height="1.8h">
					<panel name="{id}_item_1" color="white" align="center" width="100@"/>
				</panel>
				<panel x="0.2h" y="4.1h" float="right" height="1.8h" padding="0.4h" >
					<panel name="{id}_item_2" color="white" width="100@" sticky="1"/>
					<panel name="{id}_item_3" color="white" width="100@" sticky="1"/>
				</panel>
				<panel x="0.2h" y="6.1h" float="right" height="1.8h" padding="0.4h" >
					<panel name="{id}_item_4" color="white" width="100@" sticky="1"/>
					<panel name="{id}_item_5" color="white" width="100@" sticky="1"/>
				</panel>
				<panel x="0.2h" y="8.2h" float="right" height="1.8h" padding="0.4h">
					<panel name="{id}_item_6" color="white" width="100@" sticky="1"/>
					<panel name="{id}_item_7" color="white" width="100@" sticky="1"/>
				</panel>
			</panel>
			
		</panel>
	</template>
	
	<!-- containers -->
	<template name="spec_hero_frames_container" >
		<panel name="{id}" align="{align}" grow="1" width="13h">
			<panel float="bottom" grow="1" regrow="1" padding="0" visible="1" align="{align}">
				<instance name="spec_hero_frame" float="{float}" id="{id}_1" align="{align}" hflip="{hflip}" abilX="{abilX}" portraitX="{portraitX}"/>
				<instance name="spec_hero_frame" float="{float}" id="{id}_2" align="{align}" hflip="{hflip}" abilX="{abilX}" portraitX="{portraitX}"/>
				<instance name="spec_hero_frame" float="{float}" id="{id}_3" align="{align}" hflip="{hflip}" abilX="{abilX}" portraitX="{portraitX}"/>
				<instance name="spec_hero_frame" float="{float}" id="{id}_4" align="{align}" hflip="{hflip}" abilX="{abilX}" portraitX="{portraitX}"/>
				<instance name="spec_hero_frame" float="{float}" id="{id}_5" align="{align}" hflip="{hflip}" abilX="{abilX}" portraitX="{portraitX}"/>
			</panel>
		</panel>
	</template>
	
	<!-- Player Container Panel -->
	<panel name="spec_hero_frames_container_parent" y="-13.0h" grow="1" width="100%" valign="center" noclick="1" visible="1">
		<instance name="spec_hero_frames_container" id="spec_hero_frames_1" abilX="6.8h" portraitX="0.4h" align="left"/>
		<instance name="spec_hero_frames_container" id="spec_hero_frames_2" abilX="0.7h" portraitX="3.4h" align="right" float="right" hflip="1"/>
		
		<instance name="pinButton" id="spec_unitFramesPin" valign="bottom" align="left" x="1.0h" y="1.5h" width="2.5h" height="2.5h" />
	</panel>
	
	
	<!-- Game Menu -->
	<include file="/ui/specui/menu/game_menu.package"/>
	
	<!-- Minimap -->
	<panel group="gameMinimapContainers" style="gameMinimapContainers">
		<frame texture="/ui/elements:rounded_bg_white" color="#333333" borderthickness="0.5h" noclick="true"/>
		<panel style="gameMinimapHeader">			
			<panel color=".05 .05 .05 1" x="1%" y="5%" width="-2%" height="60%" noclick="1" />
			<panel style="gameMinimapIconCinema" visible="0">
				<button name="spec_replay_control_btn" color="invisible">
					<panel name="spec_replay_control_btnBody" align="center" valign="center" noclick="true">
						<frame noclick="true" texture="/ui/elements:rounded_bg_white" borderthickness="0.5h" color="0 0 0 0.5" x="0.25h" y="0.25h"/>
						<frame noclick="true" texture="/ui/elements:rounded_button_bg_white" borderthickness="0.5h" color="#444444"/>
						<frame noclick="true" texture="/ui/elements:roundframe" borderthickness="0.5h" color="black"/>
						<image name="spec_replay_control_btnPin" texture="/ui/_textures/icons/ui_playarrow.tga" align="center" valign="center" width="45@" height="45%"/>
					</panel>
				</button>
			</panel>
			<panel style="gameMinimapIconCinema"><!-- Cinematic View -->
				<button name="specCinemaModeButton"
					color="invisible"
					onmouseoverlua="
						simpleTipGrowYUpdate(true, nil, Translate('spec_cinema_mode_button'), Translate('spec_cinema_mode_button_desc', 'value', GetKeybindButton('game', 'Cmd', 'F6', 0)))
					"
					onmouseoutlua="
						simpleTipGrowYUpdate(false) 
					"
				>
					<widgetstate statename="up">						
						<panel style="gameInventoryItemIconSpace">							
							<frame texture="/ui/elements:rounded_bg_solid" color=".12 .12 .12 1" borderthickness="0.4h" noclick="true"/>
							<image texture="/ui/main/watch/textures/icon_live.tga" width="80@" height="80%" align="center" valign="center" color=".8 .8 .8 1" />
							<image texture="/ui/elements:gloss_up" rendermode="additive" color=".6 .6 .6 1" noclick="true"/>
							<frame texture="/ui/elements:rounded_corner_white" color="#333333" borderthickness="0.4h" noclick="true"/>
							<frame texture="/ui/elements:thick" color="black" borderthickness="0.4h" noclick="true"/>
						</panel>						
					</widgetstate>					
					<widgetstate statename="over">
						<panel style="gameInventoryItemIconSpace">							
							<frame texture="/ui/elements:rounded_bg_solid" color=".12 .12 .12 1" borderthickness="0.4h" noclick="true"/>
							<image texture="/ui/main/watch/textures/icon_live.tga" width="80@" height="80%" align="center" valign="center" color=".8 .8 .8 1" />
							<image texture="/ui/elements:gloss_up" rendermode="additive" color="1 1 1 1" noclick="true"/>
							<frame texture="/ui/elements:rounded_corner_white" color="#333333" borderthickness="0.4h" noclick="true"/>
							<frame texture="/ui/elements:thick" color="black" borderthickness="0.4h" noclick="true"/>
						</panel>
					</widgetstate>
					<widgetstate statename="down">
						<panel style="gameInventoryItemIconSpace">							
							<frame texture="/ui/elements:rounded_bg_solid" color=".12 .12 .12 1" borderthickness="0.4h" noclick="true"/>
							<image x="1" y="1" texture="/ui/main/watch/textures/icon_live.tga" width="80@" height="80%" align="center" valign="center" color=".8 .8 .8 1" />
							<frame texture="/ui/elements:rounded_corner_white" color="#333333" borderthickness="0.4h" noclick="true"/>
							<frame texture="/ui/elements:thick" color="black" borderthickness="0.4h" noclick="true"/>
						</panel>
					</widgetstate>
				</button>
			</panel>
			<panel style="gameMinimapIconRight"><!-- Options -->
				<button name="specgameMenuButton"
					color="invisible"
					onmouseoverlua="
						simpleTipGrowYUpdate(true, nil, Translate('game_menu_button_1'), Translate('game_menu_button_1_desc', 'value', GetKeybindButton('game', 'Cmd', 'F6', 0)))
					"
					onmouseoutlua="
						simpleTipGrowYUpdate(false) 
					"
				>
					<widgetstate statename="up">						
						<panel style="gameInventoryItemIconSpace">							
							<frame texture="/ui/elements:rounded_bg_solid" color=".12 .12 .12 1" borderthickness="0.4h" noclick="true"/>
							<image texture="/ui/game/shared/minimap/textures/mini_map_options.tga" width="80@" height="80%" align="center" valign="center" color=".8 .8 .8 1" />
							<image texture="/ui/elements:gloss_up" rendermode="additive" color=".6 .6 .6 1" noclick="true"/>
							<frame texture="/ui/elements:rounded_corner_white" color="#333333" borderthickness="0.4h" noclick="true"/>
							<frame texture="/ui/elements:thick" color="black" borderthickness="0.4h" noclick="true"/>
						</panel>						
					</widgetstate>					
					<widgetstate statename="over">
						<panel style="gameInventoryItemIconSpace">							
							<frame texture="/ui/elements:rounded_bg_solid" color=".12 .12 .12 1" borderthickness="0.4h" noclick="true"/>
							<image texture="/ui/game/shared/minimap/textures/mini_map_options.tga" width="80@" height="80%" align="center" valign="center" color=".8 .8 .8 1" />
							<image texture="/ui/elements:gloss_up" rendermode="additive" color="1 1 1 1" noclick="true"/>
							<frame texture="/ui/elements:rounded_corner_white" color="#333333" borderthickness="0.4h" noclick="true"/>
							<frame texture="/ui/elements:thick" color="black" borderthickness="0.4h" noclick="true"/>
						</panel>
					</widgetstate>
					<widgetstate statename="down">
						<panel style="gameInventoryItemIconSpace">							
							<frame texture="/ui/elements:rounded_bg_solid" color=".12 .12 .12 1" borderthickness="0.4h" noclick="true"/>
							<image x="1" y="1" texture="/ui/game/shared/minimap/textures/mini_map_options.tga" width="80@" height="80%" align="center" valign="center" color=".8 .8 .8 1" />
							<frame texture="/ui/elements:rounded_corner_white" color="#333333" borderthickness="0.4h" noclick="true"/>
							<frame texture="/ui/elements:thick" color="black" borderthickness="0.4h" noclick="true"/>
						</panel>
					</widgetstate>
				</button>
			</panel>
			

		</panel>
	</panel>	
	
	<panel group="gameMinimapContainers" style="gameMinimapContainers">
		<frame texture="/ui/elements:rounded_bg_white" color="#333333" borderthickness="0.5h" noclick="true"/>
		<panel style="gameMinimapInset">
			<map name="gameMinimap" hoversize="0.5" outlinesize="0.19h" texture="!minimap_texture" watch="Minimap" />
		</panel>
	</panel>

	<panel group="gameMinimapContainers" style="gameMinimapContainers">
		<panel style="gameMinimapInset">
			<frame texture="/ui/elements:rounded_corner_white" color="#333333" borderthickness="0.5h" noclick="true"/>
			<frame texture="/ui/elements:roundframe" color="black" borderthickness="0.5h" noclick="true"/>
		</panel>

		<panel style="gameMinimapHeader">
		
			<image width="100%" height="25@" texture="/ui/game/shared/minimap/textures/mini_map_header.tga" rendermode="grayscale" align="right" noclick="true"/>
			<panel color="#0a0a0a" x="48.5%" y="14%" width="23.6%" height="49.0%" noclick="1" />
		
			<panel style="gameMinimapIconRight">
				<panel style="gameInventoryBasicKeySpace" noclick="true">
					<instance id="specgameMenuButtonKey" name="gameBindableHotkeyButton" visible="0"
						font="maindyn_9"
						onrefreshlua="
							local button = GetKeybindButton('game', 'TriggerToggle', 'gameToggleMenu')
							if button then
								self:SetText(button)
							else
								self:SetText('')
							end
						"
					/>
				</panel>
			</panel>
