<?xml version="1.0" encoding="UTF-8"?>
<effect deferred="true" useentityeffectscale="true">

	<definitions>

		<modifier
			name="modifier0"
			color="1 1.5 2"
			startalpha="1"
			midalpha="1"
			endalpha="1"
		/>

		<particlesystem name="system0" space="world" scale=".75">
		
		
		
		
				<terrainemitter
					position="0 0 0"
					delay="0"
					spawnrate="25"
					particlelife="1500"
					range="25"
				>
					<particle>
						<groundsprite
							minlife="250"
							maxlife="750"
							material="/shared/effects/smaterials/water_overlay.material"
							minstartsize="25"
							maxstartsize="50"
							minendsize="70"
							maxendsize="90"
							midsizepos=".25"
							minyaw="-360"
							maxyaw="360"
							color="0 .35 1"
							startalpha=".2"
							midalpha=".1"
							endalpha="0"
							midalphapos=".5"
						/>
						
						<billboard
							minlife="250"
							maxlife="750"
							material="/shared/effects/smaterials/whitedot.material"
							minstartsize="25"
							maxstartsize="150"
							minendsize="75"
							maxendsize="150"
							midsizepos=".25"
							startcolor=".025 .075 .125"
