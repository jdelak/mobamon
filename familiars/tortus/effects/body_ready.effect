							lockup="1"
							lockright="1"
							pitch="90"
							depthbias="-25"
						/>
					</particle>
				</terrainemitter>
		
		
		
		
		
		
		
		
			<template name="splashes">

				<simpleemitter
					spawnrate="10"
					particlelife="1000"
					offsetsphere="0 0 0"
					position="{zpos}"
					speed="50"
				>
					<particle
					>
					
						<simpleemitter
						count="25"
							spawnrate="3000"
							minparticlelife="250"
							maxparticlelife="500"
							material="/shared/effects/smaterials/water_white.material"
							offsetsphere="15 15 0"
							position="0 0 0"
							speed="50"
						>
							<particle
								startcolor="1 1 1"
								endcolor="0 .5 1"
								startsize="50"
								maxangle="360"
								minendsize="50"
								maxendsize="100"
								startalpha=".04"
								endalpha="0"
							/>
						</simpleemitter>

					</particle>
				</simpleemitter>
				
				
				


			</template>

			<instance name="splashes" zpos="20 15 0" />
			<instance name="splashes" zpos="-20 15 0" />
			
			
			
			
			
			
			
			
			
			

		</particlesystem>
	</definitions>
	<thread>
		<spawnparticlesystem instance="instance0" particlesystem="system0"/>
		<spawnmodifier instance="instance1" modifier="modifier0"/>
		<waitfordeath instance="instance0" />
		<waitfordeath instance="instance1" />
	</thread>
</effect>
