<?xml version="1.0" encoding="UTF-8"?>
<model name="rig_gate.max" file="model.model" type="K2">

	<anim name="idle" clip="default_1.clip" loop="true"/>

	<anim name="open_doors" clip="open.clip" loop="true" loopbackframe="20" >
		<frameevent frame="1" cmd="PlaySoundLinear sounds/sfx_door_jail.wav 500 2400 .75"/>
	</anim>

	<anim name="close_doors" clip="close.clip" loop="false" />
	
	<anim name="blow_open" clip="bash_1.clip" fps="60" loop="true" loopbackframe="250" />
	
</model>
