<?xml version="1.0" encoding="UTF-8"?>
<material>
	<parameters shader="/shared/shaders/unit_opacity_rimlight_params.shader" vDiffuseColor="1 1 1" fSpecularLevel="2.0" fGlossiness="18.0" fOpacity="1.0" fReflect="1.0" >
		<vec4 name="vFresnelColor" value=".3 0 0 0" />
		<vec3 name="vFresnelScale" value=".25 5 5" />
	</parameters>
	
	<phase name="shadow" cull="back" blend="alphatest">
		<sampler name="diffuse" texture="/world/sprops/generator/color.tga" repeat_u="true" repeat_v="true" />
	</phase>
	<phase name="color" cull="back" blend="alphatest">
		<sampler name="diffuse" texture="/world/sprops/generator/color.tga" repeat_u="true" repeat_v="true" />
		<sampler name="normalmap" texture="/world/sprops/generator/normal.tga" repeat_u="true" repeat_v="true" />
		<samplercube name="cube" texture="/world/sky/gold/1.tga" />
		<sampler name="specular" texture="/world/sprops/generator/spec_color.tga" repeat_u="true" repeat_v="true" />
		<sampler name="team" texture="/world/sprops/generator/team.tga" repeat_u="true" repeat_v="true" />
	</phase>
	<phase name="fade" cull="back" colorwrite="false" alphawrite="false" depthwrite="true" blend="alphatest">
		<sampler name="diffuse" texture="/world/sprops/generator/color.tga" repeat_u="true" repeat_v="true" />
		<multipass cull="back" blend="translucent">
			<sampler name="diffuse" texture="/world/sprops/generator/color.tga" repeat_u="true" repeat_v="true" />
			<sampler name="normalmap" texture="/world/sprops/generator/normal.tga" repeat_u="true" repeat_v="true" />
			<samplercube name="cube" texture="/world/sky/gold/1.tga" />
			<sampler name="specular" texture="/world/sprops/generator/spec_color.tga" repeat_u="true" repeat_v="true" />
			<sampler name="team" texture="/world/sprops/generator/team.tga" repeat_u="true" repeat_v="true" />
		</multipass>
	</phase>
	
</material>
