	<template name="socialclient_im_friendlist_row_template" >
		<panel name="socialclient_im_friendlist_row_{index}" group="socialclient_im_friendlist_row_group" noclick="1" float="right" padding="0s" sticky="1" stickytoinvis="0" grow="1" regrow="1" growinvis="0" />
	</template>

	<template name="socialclient_im_friendlist_droptarget_template" >
		<panel height="40s" noclick="1" width="258s" sticky="1" stickytoinvis="0">
			<panel name="socialclient_im_friendlist_droptarget_{index}" color="invisible" height="40s" noclick="0" width="100%" oninstantiatelua="" sticky="1" stickytoinvis="0">

				<frame width="254s" texture="/ui/elements:rounded_bg_solid" color="0.02 0.07 0.09 0.98" bordercolor="0.02 0.07 0.09 0.98" borderthickness="3s" noclick="1"/>
				<panel x="5s" height="80%" valign="center" float="right" padding="3s" noclick="1" >
					<panel width="150s" noclick="1" >
						<label content="{label=???}" x="5s" y="-2s" fity="1" fitx="1" font="maindyn_18" align="left" textalign="left" valign="center" noclick="1" color="#0cb9e5" />
					</panel>
				</panel>			
				
				<instance name="DropTarget" index="socialclient_im_friendlist_droptarget_{index}" width="100%" height="100%" visible="1"/>
				
			</panel>
		</panel>
	</template>	
	
	<template name="socialclient_friendlist_template" >
		<panel name="socialclient_im_friendlist_parent" noclick="1" height="-106s" valign="top" y="69s" sticky="0" stickytoinvis="0" width="-30s" clip="0" style="simpleListbox"
		>
			<panel name="socialclient_im_friendlist" noclick="1" valign="top" sticky="0" stickytoinvis="0" style="simpleListbox" float="bottom" padding="4s"
			>
			<!-- 15s space should be between each group, but not each friend in the list -->
			</panel>
		</panel>
		<scrollbar name="socialclient_im_friendlist_scrollbar" visible="0"
			texture="/ui/shared/textures/new_slider.tga"
			vertical="true"					
			width="16s" align="right" valign="top" height="-106s" step="1" minvalue="0" maxvalue="6"
			hscrollbar="0"
			horizontal="false"	
			handleheight="50%" y="69s" x="-2s"			
		/>		
		<panel noclick="0" blockinput="0" processonlyscroll="1" hover="false" group="social_client_scroll_catchers"
			onmousewheeldownlua=""
			onmousewheeluplua=""										
		/>
	</template>


</package>
