	<template name="socialclient_im_header_row_template" >
		<panel name="socialclient_im_header_row_template{index}" height="28s" width="100%" float="right" padding="5s" noclick="0" oninstantiatelua="Friends['{interfaceName}'].RegisterHeader(self, '{index}', '{displayGroup}')" sticky="1" stickytoinvis="0" clip="1" >
			<panel name="socialclient_im_header_row_template{index}_arrow" rotation="{rotation=0}" x="5s" height="18s" width="18s" noclick="1" color="#cfd3e7" texture="/ui/main/shared/textures/drop_arrow.tga" valign="center"/>
			<label name="socialclient_im_header_row_template{index}_label" content="{label=Header Name Is Missing}" font="maindyn_18" color="#cfd3e7" fitx="1" fity="1" align="left" valign="center" noclick="1" />
			<label name="socialclient_im_header_row_template{index}_count_label" content="- {countLabel=?/?}" font="maindyn_14" color="0.81 0.83 0.91 0.3" fitx="1" fity="1" align="left" valign="center" noclick="1" />
			<panel height="2s" width="120s" align="center" valign="center" color="1 1 1 0.21" texture="/ui/main/shared/textures/grad_horz.tga" noclick="1"/> <!-- This line will need to be calculated based on the width of the arrow, the width of the group name, the width of the users within the group, and the remaining list width -->
		</panel>
	</template>
	
